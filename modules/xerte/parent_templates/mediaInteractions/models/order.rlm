	<PG name="dictation">		<SCR name="getPageID"><![CDATA[pageIcon = icon.parentNode.clip();myXML = new XML(extXML.firstChild.childNodes[rootIcon.getPageIndex() - pgOffset]);pageIcon.templateData = myXML.toObject();rootIcon.setTitleText(templateData.learningObject[0].name);if (pageIcon.templateData.order[0].image != undefined){  fLoadImage(pageIcon.templateData.order[0].image);} else {  fShowMedia(pageIcon.templateData.order[0].showMedia);}]]></SCR>		<DIS id="instructions" name="intructions" x="380" y="20" w="400" h="10" type="text"><![CDATA[{pageIcon.templateData.order[0].instructions}]]></DIS>		<SCR name="makePages"><![CDATA[if (icon.built != true){  icon.nextSibling.firstChild.duplicate(trans.p.length - 1);  icon.built = true;}]]></SCR>		<DEC name="pages" repeat="all" branch="randomUnused" pause="0" erase="1" eraseOnExit="1">			<PG name="protoPage">				<SCR name="getIndex"><![CDATA[chIndex = icon.parentNode.index();]]></SCR>				<DIS textColour="#333333" textSize="16" textFont="Courier" id="words" name="words" x="380" y="instructions._y + instructions._height + 60" w="400" h="10" type="text"><![CDATA[{trans.p[chIndex]}]]></DIS>				<SCR name="break"><![CDATA[wordMCs = words.breakApart(10,10,true,true);tempH = words._height;currentX = 0;currentY = tempH + 10; bX = 0;bY = 0;wordMCs.randomize();for (i = 0; i < wordMCs.length; i++){    tempW = wordMCs[i];  tempW._x = bX;  tempW._y = bY;    tempW.beginFill(0xFFFFCC,100);  tempW.drawRect(0,0,tempW._width, tempW._height);    bX += tempW._width + 3;  if (bX >= 310){    bX = 0;    bY += 22;  }  //for replacing...  tempW.rndX = wordMCs[i]._x;  tempW.rndY = wordMCs[i]._y; }dropped = new Array();words.createEmptyMovieClip('target', -9);words.target._x = 0;words.target._y = tempH + 10;words.target.beginFill(0xFFFFFF,100);words.target.lineStyle(1,0x000000,100);words.target.drawRect(-1,-1,400,tempH+2);]]></SCR>				<DIS id="fb" name="feedback" x="380" y="instructions._y + instructions._height + 60 + tempH*2 + 20" w="400" h="10" type="text"><![CDATA[Drag the words into the space in the right order]]></DIS>				<INT name="interaction" perpetual="0">					<RES swf="templatePath + 'common/button.swf'" name="playChunk" type="button" x="380" y="instructions._y + instructions._height + 20" label="Listen" erase="1" exit="0">						<SCR name="playChunk"><![CDATA[playChunk(chIndex);]]></SCR>					</RES>					<RES swf="templatePath + 'common/button.swf'" name="answers" type="button" x="480" y="instructions._y + instructions._height + 20" label="Check" erase="1" exit="0">						<SCR name="check"><![CDATA[//check over dropped, matching index. replace after first errornumCorrect = -1;for (i = 0; i < dropped.length; i++){  if (dropped[i].index != i){    break;  } else {    numCorrect = i;  }}//leave the correct words in place - replace after the first errorfor (i = numCorrect + 1; i < dropped.length; i++){  dropped[i].animate(dropped[i].rndX,dropped[i].rndY, 5,'linearTween');  dropped[i].draggable = true;  dropped[i].useHandCursor = true;}//remove the incorrect items from droppeddropped.splice(numCorrect + 1);if (dropped.length > 0){  currentX = dropped[dropped.length - 1]._x + dropped[dropped.length - 1]._width + 3;  currentY = dropped[dropped.length - 1]._y;} else {  currentX = 0;  currentY = tempH + 10; }if(numCorrect + 1 < wordMCs.length){  fb.setText('The first ' + (numCorrect + 1) + ' words are in the correct place.');} else {  fb.setText('Well done - you have placed all the words correctly.');}]]></SCR>					</RES>					<RES swf="templatePath + 'common/button.swf'" name="next" type="button" x="580" y="instructions._y + instructions._height + 20" label="Next" erase="1" exit="1">						<SCR name="exit"><![CDATA[stopChunk();]]></SCR>					</RES>					<RES name="wordDropped" type="event" eventName="onWordDropped" icon="words" erase="1" exit="0">						<SCR name="handleDrop"><![CDATA[if (words.wordDropped.hitTest(words.target) == true){    if (currentX + words.wordDropped._width > 400){    currentX = 0;    currentY += 22;  }    words.wordDropped.animate(currentX,currentY,5,'linearTween');  currentX += words.wordDropped._width;    dropped.push(words.wordDropped);  words.wordDropped.draggable = false;  words.wordDropped.useHandCursor = false;  } else {  words.wordDropped.rndX = words.wordDropped._x;  words.wordDropped.rndY = words.wordDropped._y;  }]]></SCR>					</RES>				</INT>			</PG>		</DEC>		<DIS name="feedback" x="380" y="instructions._y + instructions._height + 20" w="400" h="10" type="text"><![CDATA[{pageIcon.templateData.order[0].feedback}]]></DIS>	</PG>