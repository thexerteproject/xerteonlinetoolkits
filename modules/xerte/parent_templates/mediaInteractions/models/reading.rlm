	<PG name="reading">		<SCR name="getPageID"><![CDATA[pageIcon = icon.parentNode.clip();myXML = new XML(extXML.firstChild.childNodes[rootIcon.getPageIndex() - pgOffset]);pageIcon.templateData = myXML.toObject();rootIcon.setTitleText(templateData.learningObject[0].name);if (pageIcon.templateData.reading[0].image != undefined){  fLoadImage(pageIcon.templateData.reading[0].image);} else {  fShowMedia(pageIcon.templateData.reading[0].showMedia);}]]></SCR>		<DIS id="instructions" name="intructions" x="380" y="20" w="400" h="10" type="text"><![CDATA[{pageIcon.templateData.reading[0].instructions}]]></DIS>		<SCR name="makePages"><![CDATA[if (icon.built != true){  icon.nextSibling.firstChild.duplicate(pageIcon.templateData.reading[0].exercise.length - 1);  icon.built = true;}]]></SCR>		<DEC name="pages" repeat="all" branch="randomUnused" pause="0" erase="1" eraseOnExit="1">			<PG name="protoPage">				<SCR name="getIndex"><![CDATA[fIndex = icon.parentNode.index();]]></SCR>				<DIS textColour="#333333" scrolling="auto" id="lang1" name="language1" x="380" y="instructions._y + instructions._height +20" w="400" h="100" type="text"><![CDATA[{pageIcon.templateData.reading[0].exercise[fIndex].lang1}]]></DIS>				<DIS textColour="#333333" scrolling="auto" id="lang2" name="language2" x="380" y="lang1._y + 120" w="400" h="100" type="text"><![CDATA[{pageIcon.templateData.reading[0].exercise[fIndex].lang2}]]></DIS>				<SCR name="draw"><![CDATA[lang1.beginFill(0xFFFFFF,100);lang1.lineStyle(1, 0x333333,100);lang1.drawRect(0,0,400,100);lang2.beginFill(0xFFFFFF,100);lang2.lineStyle(1, 0x333333,100);lang2.drawRect(0,0,400,100);]]></SCR>				<INT name="interaction" perpetual="0">					<RES rs="btnNext" swf="templatePath + 'common/button.swf'" name="next" type="button" x="380" y="lang2._y + 120" label="Next" erase="1" exit="1">						<SCR name="nextPage"><![CDATA[btnNext.setEnabled(false,false);]]></SCR>					</RES>				</INT>			</PG>		</DEC>		<DIS id="feedback" name="feedback" x="380" y="instructions._y + instructions._height + 20" w="400" h="10" type="text"><![CDATA[{pageIcon.templateData.reading[0].feedback}]]></DIS>		<BIN name="Deleted Items" />	</PG>