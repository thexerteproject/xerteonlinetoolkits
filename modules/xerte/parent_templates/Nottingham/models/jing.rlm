	<PG name="jing">		<SCR name="getPageID"><![CDATA[pageIcon = icon.parentNode.clip();myXML = new XML(extXML.firstChild.childNodes[rootIcon.getPageIndex() - pgOffset]);pageIcon.templateData = myXML.toObject();rootIcon.setTitleText(templateData.learningObject[0].name);if (pageIcon.templateData.captivate[0].narration != undefined){  narration = pageIcon.templateData.jing[0].narration;  rootIcon.broadcast('showAudio');}sm = Stage.scaleMode;]]></SCR>		<DIS id="jng" name="jing" x="20" y="20" type="ext" url="{pageIcon.templateData.jing[0].url}"><![CDATA[]]></DIS>		<INT name="loader" perpetual="0">			<RES name="onLoad" type="event" eventName="onLoad" icon="jng" erase="1" exit="0">				<SCR name="handle"><![CDATA[jng._lockroot = true;initWidth = jng._width;jng.constrain(760 * jng.clip._width / jng.clip.width, 474 * jng.clip._height / jng.clip.height);jng._x = 400 - (jng.clip.width * (jng._width / initWidth) / 2);Stage.scaleMode = sm;]]></SCR>			</RES>		</INT>	</PG>