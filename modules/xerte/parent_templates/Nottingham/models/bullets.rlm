	<PG title="Bullets" name="Bullets">		<SCR name="getPageID"><![CDATA[pageIcon = icon.parentNode.clip();if (rootIcon.projMode == "dev"){   // Page Wizard  pageIcon.templateData = pageIcon.extXML.toObject();  pageIcon.templateData = pageIcon.templateData.pageWizard[0];} else {  // Toolkits  myXML = new XML(extXML.firstChild.childNodes[rootIcon.getPageIndex() - pgOffset]);  pageIcon.templateData = myXML.toObject();}	  rootIcon.setTitleText(templateData.learningObject[0].name);  // handle multiple page data definitions in a project if (rootIcon.projectPageIndex == undefined){     rootIcon.projectPageIndex = 0;}// check if this page has a definition for the current project index and default to 0 if notif (pageIcon.templateData.bullets[rootIcon.projectPageIndex].name == undefined){  projPI = 0;} else {  projPI = rootIcon.projectPageIndex; }  if (pageIcon.templateData.bullets[projPI].delaySecs != undefined){  delaySecs = parseInt(pageIcon.templateData.bullets[projPI].delaySecs) / 10;} else {  delaySecs = "0.1"; // original page has no delay}if (pageIcon.templateData.bullets[projPI].showBullets != undefined){  showBullets = pageIcon.templateData.bullets[projPI].showBullets;} else {  showBullets = "0"; // to be consistent with original bullets page}if (pageIcon.templateData.bullets[projPI].showPanel != undefined){  showPanel = pageIcon.templateData.bullets[projPI].showPanel;} else {  showPanel = "0"; // to be consistent with original bullets page}if (pageIcon.templateData.bullets[projPI].panelWidth != undefined){  panelWidth = pageIcon.templateData.bullets[projPI].panelWidth;} else {  panelWidth = "Large";}if (panelWidth == "Small"){  panelW = 300;} else if (panelWidth == "Medium"){  panelW = 450;} else if (panelWidth == "Large"){  panelW = 600;} else {  panelW = 760;}textW = panelW - 20;if (pageIcon.templateData.bullets[projPI].narration != undefined){  narration = pageIcon.templateData.bullets[projPI].narration;  rootIcon.broadcast('showAudio');}]]></SCR>		<DIS h="474" w="panelW" tabIndex="-1" id="panel" name="panel" x="20" y="20" type="ext" url="templatePath + 'common/whitePanel.swf'"><![CDATA[]]></DIS>		<SCR name="panel and bullet setup"><![CDATA[// bullets = pageIcon.templateData.bullets[projPI].split('\r\r');bullets = pageIcon.templateData.bullets[projPI].split('\r');if (icon.attributes.built != 'true'){  icon.nextSibling.firstChild.duplicate(bullets.length - 1);  icon.attributes.built = 'true';}if (showPanel == "1"){  panel._visible = true;  nextY = 25;} else {  panel._visible = false;  nextY = 20;}]]></SCR>		<DEC name="bullets" repeat="all" branch="sequence" pause="0" erase="0" eraseOnExit="0">			<PG name="protoBullet">				<DIS id="b" transition="wipe('in',0.3,'None','easeNone',4)" scrolling="auto" name="bullet" x="20" y="nextY" w="textW" h="475" type="text"><![CDATA[{bullets[icon.parentNode.index()]}]]></DIS>				<SCR name="getNextY"><![CDATA[// set up current bullet{bullets[icon.parentNode.index()]}if (showBullets == "1"){  b.setText("<li>" + bullets[icon.parentNode.index()] + "</li>");} else {  b.setText(bullets[icon.parentNode.index()]);}// set up y for next bulletnextY = b._y + b._height + 10;]]></SCR>				<INT name="pause" perpetual="0">					<RES name="onMotionFinished" type="event" eventName="onMotionFinished" icon="b" erase="1" exit="1">						<SCR name="exit"><![CDATA[//nothing to see here]]></SCR>						<INT name="Timer" perpetual="0">							<RES name="Time delay" type="timeLimit" secs="delaySecs" reset="0" erase="1" exit="1">								<SCR name="Another exit"><![CDATA[// carry on]]></SCR>							</RES>						</INT>					</RES>				</INT>			</PG>		</DEC>	</PG>