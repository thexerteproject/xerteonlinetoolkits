	<PG title="Hangman" name="hangman">		<SCR name="getPageID"><![CDATA[pageIcon = icon.parentNode.clip();if (rootIcon.projMode == "dev"){   // Page Wizard  pageIcon.templateData = pageIcon.extXML.toObject();  pageIcon.templateData = pageIcon.templateData.pageWizard[0];} else {  // Toolkits  myXML = new XML(extXML.firstChild.childNodes[rootIcon.getPageIndex() - pgOffset]);  pageIcon.templateData = myXML.toObject();}  rootIcon.setTitleText(templateData.learningObject[0].name);  // handle multiple page data definitions in a project if (rootIcon.projectPageIndex == undefined){     rootIcon.projectPageIndex = 0;}// check if this page has a definition for the current project index and default to 0 if notif (pageIcon.templateData.hangman[rootIcon.projectPageIndex].name == undefined){  projPI = 0;} else {  projPI = rootIcon.projectPageIndex; }  // set up text stringsif (pageIcon.templateData.hangman[projPI].nextBtnTxt != undefined){  nextBtnTxt = pageIcon.templateData.hangman[projPI].nextBtnTxt;} else {  nextBtnTxt = "Next";}if (pageIcon.templateData.hangman[projPI].nextBtnTip != undefined){  nextBtnTip = pageIcon.templateData.hangman[projPI].nextBtnTip;} else {  nextBtnTip = "Next Question";}if (pageIcon.templateData.hangman[projPI].nextBtnWidth != undefined){  nextBtnWidth = pageIcon.templateData.hangman[projPI].nextBtnWidth;} else {  nextBtnWidth = "100";}if (pageIcon.templateData.hangman[projPI].beforeQuesCount != undefined){  beforeQuesCount = pageIcon.templateData.hangman[projPI].beforeQuesCount;} else {  beforeQuesCount = "Question";}if (pageIcon.templateData.hangman[projPI].nQValue != undefined){  nQValue = pageIcon.templateData.hangman[projPI].nQValue;} else {  nQValue = "n";}if (pageIcon.templateData.hangman[projPI].betweenQuesCount != undefined){  betweenQuesCount = pageIcon.templateData.hangman[projPI].betweenQuesCount;} else {  betweenQuesCount = "of";}if (pageIcon.templateData.hangman[projPI].afterQuesCount != undefined){  afterQuesCount = pageIcon.templateData.hangman[projPI].afterQuesCount;} else {  afterQuesCount = "";}// end of text string setupif (pageIcon.templateData.hangman[projPI].narration != undefined){  narration = pageIcon.templateData.hangman[projPI].narration;  rootIcon.broadcast('showAudio');]]></SCR>		<DIS id="instr" scrolling="auto" name="text" x="20" y="20" w="400" h="475" type="text"><![CDATA[{pageIcon.templateData.hangman[projPI].text}]]></DIS>		<SCR name="setup"><![CDATA[words = pageIcon.templateData.hangman[projPI].words.split('\r');if (icon.built != 'true'){  icon.nextSibling.childNodes[1].duplicate(words.length - 1);  icon.built = 'true';}]]></SCR>		<FW id="wordsFW" name="words">			<FW_ENT name="controls">				<DIS id="otn" name="orientation" x="20" y="400" w="760" h="10" type="text"><![CDATA[{'<p align="center">Exercise 1 of ' + words.length + '</p>'}]]></DIS>				<SCR name="[Untitled]"><![CDATA[	// set up display message	if(nQValue == "n"){	  // item number before count	  // otn.setText('<p align="center">Exercise ' + (wordsFW.currentPage + 1) + ' of ' + words.length + '</p>');	  otn.setText('<p align="center">' + beforeQuesCount + ' 1 ' + betweenQuesCount + ' ' + words.length + ' ' + afterQuesCount + '</p>');	} else {	  // count before item number		  otn.setText('<p align="center">' + beforeQuesCount + ' ' + words.length + ' ' + betweenQuesCount + ' 1 ' + afterQuesCount + '</p>');						  	}]]></SCR>				<INT name="controls" perpetual="0">					<RES width="nextBtnWidth" toolTip="{nextBtnTip}" enabled="false" rs="btnNext" swf="templatePath + 'common/button.swf'" name="next" type="button" x="360" y="420" label="{nextBtnTxt}" erase="1" exit="0">						<SCR name="next"><![CDATA[if (wordsFW.currentPage == words.length - 1){  wordsFW.gotoPage(0);} else {  wordsFW.nextPage();}cpad._alpha = 100;]]></SCR>					</RES>					<RES name="[Untitled]" type="event" eventName="onOpenPage" icon="wordsFW" erase="1" exit="0">						<SCR name="[Untitled]"><![CDATA[// set up display message	if(nQValue == "n"){	  // item number before count	  // otn.setText('<p align="center">Exercise ' + (wordsFW.currentPage + 1) + ' of ' + words.length + '</p>');	  otn.setText('<p align="center">' + beforeQuesCount + ' ' + (wordsFW.currentPage + 1) + ' ' + betweenQuesCount + ' ' + words.length + ' ' + afterQuesCount + '</p>');	} else {	  // count before item number		  otn.setText('<p align="center">' + beforeQuesCount + ' ' + words.length + ' ' + betweenQuesCount + ' ' + (wordsFW.currentPage + 1) + ' ' + afterQuesCount + '</p>');						  	}		/*	// set up display message	if(nQValue == "n"){	  // item number before count	  // otn.setText('<p align="center">Exercise ' + (wordsFW.currentPage + 1) + ' of ' + words.length + '</p>');	  otn.setText("<p align="center"> + beforeQuesCount + " 1 " + betweenQuesCount + " " + words.length + " " + afterQuesCount + "</p>");	} else {	  // count before item number		  otn.setText("<p align="center"> + beforeQuesCount + " " + words.length + " " + betweenQuesCount + " 1 " + afterQuesCount + "</p>");						  	}	*/				]]></SCR>					</RES>				</INT>			</FW_ENT>			<PG name="protoGame">				<SCR name="setup"><![CDATA[btnNext.setEnabled(false);word = words[icon.parentNode.index() - 1];dis = '';for (i = 0; i < word.length; i++){  dis = dis + '-';}usedLetters = new Object();usedStr = '';]]></SCR>				<DIS id="w" textSize="20" textFont="Courier" name="word" x="20" y="instr._y + instr._height + 30" w="760" h="10" type="text"><![CDATA[{'<b><p align="center">' + dis + '</p></b>'}]]></DIS>				<DIS id="hangGraphics" name="graphics" x="280" y="w._y + w._height + 10" type="ext" url="templatePath + 'common/hangman.swf'"><![CDATA[]]></DIS>				<DIS textFont="Courier" textSize="20" id="used" name="usedLetters" x="280" y="_ytile + 200" w="200" h="10" type="text"><![CDATA[]]></DIS>				<INT name="mainInteraction" perpetual="0">					<RES name="onKeyDown" type="event" eventName="onKeyDown" icon="rootIcon" erase="1" exit="0">						<SCR name="calculate"><![CDATA[//get key pressedl = String.fromCharCode(Key.getAscii()).toLowerCase(); found = false;if (usedLetters[l] == undefined && dis.indexOf('-') != -1 && usedStr.length < 10){    temp = '';  for (i = 0; i < dis.length; i++){    if (word.substr(i,1).toLowerCase() == l){      temp += l;      found = true;    } else {      temp += dis.substr(i,1);    }  }    dis = temp;  w.setText('<b><p align="center">' + dis + '</p></b>');    if (found == false){    usedLetters[l] = true;    usedStr += l;    used.setText(usedStr);    hangGraphics.clip.gotoAndStop(usedStr.length + 1);  }}if (dis.indexOf('_') == -1){  btnNext.setEnabled(true);  w.setText('<b><p align="center">' + dis + '</p></b>');}if (usedStr.length == 10){  btnNext.setEnabled(true);  w.setText('<b><p align="center">' + word + '</p></b>');}]]></SCR>					</RES>				</INT>			</PG>		</FW>	</PG>