<wizard menus="Interactivity">  <pageWizard remove="true" duplicate="false">    <newNodes>          <interactiveTable><![CDATA[<interactiveTable name="Enter Page Title" text="" align="Left" data="1|2|3" panelWidth="auto" submitLabel="Submit" continueLabel="Continue" correctTxt="Your answer is correct" incorrectTxt="Your answer is incorrect" correctFbTxt="The correct answer is" srCorrectTxt="Incorrect submitted answer" srIncorrectTxt="Correct answer" incomplete="Complete the exercise"/>]]></interactiveTable>    </newNodes>  	    </pageWizard>    <!-- INTERACTIVE TABLE=================================================== -->  <interactiveTable menu="Interactivity" menuItem="Interactive Table" hint="A page for creating an interactive table where table cells can contain questions or buttons that show additional information" icon="icTableEdit" thumb="thumbs/interactiveTable.jpg" example="https://xot.xerte.org.uk/play_116?hide=bottom#interactiveTable" wiki="https://xerte.org.uk/wiki/doku.php?id=interactivity_interactiveTable_page" remove="true" deepLink="true">    <name label="Page Title" type="TextInput" wysiwyg="true" />    <text label="Page Text" type="TextArea" height="100" placeholder="Enter text for the page here" />    <align label="Align Text" options="Left,Right" type="ComboBox" data="Left,Right"/>    <data label="Table Data" newRow="data,data" newCol="data" type="DataGrid" height="190" width="390" columns="2" editable="1,1" controls="1" addCols="true" showHeaderRow="false" wysiwyg="true" tooltip="Create a table containing the data shown before any interactive cells are triggered. The correct number of columns and rows must be created within this table, even when cells will remain empty until interacted with." />    <panelWidth label="Table Width" options="Automatic,Small,Medium,Large,Full" type="ComboBox" data="auto,small,medium,large,full" tooltip="When set to small, medium or large, the selected size will be overridden if needed to display the table's content"/>    <caption label="Table Caption" type="TextInput" wysiwyg="true" optional="true" tooltip="A caption which will appear above the table" />    <display type="group" label="Table Appearance" optional="true">      <borders label="Borders" type="ComboBox" options="None,Full grid,Simple,Horizontal only,Vertical only,Outside border" data="none,full,simple,horizontal,vertical,outside" defaultValue="full" optional="true"/>      <shade label="Shade Alternate Rows" type="Checkbox" defaultValue="false" optional="true"/>      <headFootGroup label="Header / Footer" type="group" optional="true">        <header label="Header Cells" options="None,First Row,First Column,First Row and Column" type="ComboBox" data="none,row,col,both" defaultValue="none" conditionTrigger="true" optional="true"/>        <footer label="Footer Row" type="CheckBox" defaultValue="false" conditionTrigger="true" optional="true"/>        <shadeHeader label="Shade Header / Footer" type="Checkbox" defaultValue="false" condition="header!='none'||footer!='false'" optional="true"/>      </headFootGroup>    </display>    <iconGroup label="Icons" type="group" optional="true" tooltip="Customise the clickable icons shown within interactive cells">      <qIcons label="Question" type="group" optional="group">        <qIconLabel label="Tooltip" type="TextInput" defaultValue="Question" optional="true" tooltip="The tooltip description for the question icons"/>        <qCustomIcon label="Customise Icon" type="CheckBox" defaultValue="false" optional="true" conditionTrigger="true" tooltip="Override the default icon used to indicate this cell is interactive. When checked, you will see a further option allowing you to select the icon used."/>        <qIcon label="Icon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-question" optional="true" condition="qCustomIcon=='true'" />      </qIcons>      <infoIcons label="Information" type="group" optional="group">        <infoIconLabel label="Tooltip (cell)" type="TextInput" defaultValue="Reveal information" optional="true" tooltip="The tooltip description for the information buttons when they reveal extra information in the table cells"/>        <infoIconLabel2 label="Tooltip (lightbox)" type="TextInput" defaultValue="Learn more" optional="true" tooltip="The tooltip description for the information buttons when they reveal extra information in lightboxes"/>        <infoIconTick label="Tick When Viewed" type="CheckBox" defaultValue="true" optional="true" tooltip="By default, the icon on the button will change to a tick after information has been viewed in a lightbox"/>        <infoCustomIcon label="Customise Icon" type="CheckBox" defaultValue="false" optional="true" conditionTrigger="true" tooltip="Override the default icon used to indicate this cell is interactive. When checked, you will see a further option allowing you to select the icon used."/>        <infoIcon label="Icon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-info" optional="true" condition="infoCustomIcon=='true'" />      </infoIcons>    </iconGroup>    <reveal label="Reveal Interactive Cells" type="ComboBox" options="All at once,One at a time,One row at a time,One column at a time" data="all,one,row,col" defaultValue="all" conditionTrigger="true" optional="true"/>    <attempts label="Attempts Allowed" defaultValue="2" type="NumericStepper" min="0" max="10" step="1" optional="true" tooltip="The number of attempts allowed for each question before the correct answer is shown. When attempts is set to zero, an unlimited amount of attempts is allowed and the correct answer will never be shown when an incorrect answer has been submitted." />    <feedback label="Feedback" type="TextArea" height="40" optional="true" tooltip="Feedback shown when after all cells have been interacted with"/>    <delimiters type="group" label="Delimiters" optional="true">      <answerDelimiter label="Correct Answer" type="textInput" defaultValue="," optional="true" tooltip="The character separating the possible correct answers for each question"/>      <dropDownDelimiter label="Drop Down Menu Option" type="textInput" defaultValue="/" optional="true" tooltip="The character separating the options that will appear within a drop down menu, e.g. England/Scotland/Wales, where the first in the list is the correct answer"/>    </delimiters>    <trackingWeight label="Tracking Score Weight" optional="true" type="textInput" defaultValue="1.0" tooltip="Customise the weight of the task on this page, relative to other scored pages in the project. Setting the weight to 0 will mean that the task on this page will not contribute to the project's overall score." />    <submitLabel label="Submit Label" type="TextInput" wysiwyg="true" divider="true" language="true"/>    <continueLabel label="Continue Label" type="TextInput" wysiwyg="true" language="true"/>    <correctTxt label="Correct Answer" type="TextInput" wysiwyg="true" language="true"/>    <incorrectTxt label="Incorrect Answer" type="TextInput" wysiwyg="true" language="true"/>    <correctFbTxt label="Correct Answer Feedback" type="TextInput" wysiwyg="true" language="true"/>    <incomplete label="Incomplete Text" type="TextInput" wysiwyg="true" language="true" mandatory="true" defaultValue="Complete the exercise"/>    <srCorrectTxt label="Correct Answer (screen readers only)" type="TextInput" language="true"/>    <srIncorrectTxt label="Incorrect Answer (screen readers only)" type="TextInput" language="true"/>    <newNodes>      <tableCell><![CDATA[<tableCell name="Cell" fieldPos="" cellType="question" text="" showIn="cell" replace="end" required="false" answer="" type="text" qShowIn="cell" qText="" qAfter="true" qAfterInfo="submitted" />]]></tableCell>    </newNodes>	  </interactiveTable>  <tableCell menuItem="Interactive Cell" icon="icBullet" remove="true">    <name label="Title" type="TextInput" tooltip="When this interactive cell contains a question, the title is used to identify this answer on the results page and when tracking results, e.g. via SCORM/LTI/xAPI"/>    <fieldPos label="Position" type="TextInput" tooltip="The position of this interactive cell within the table, using the format: column,row, e.g. 1,1"/>    <cellType label="Interactivity" type="ComboBox" options="Question,Information" data="question,info" defaultValue="" conditionTrigger="true"/>    <information label="Information" type="group" tooltip="Display additional information when the table cell is selected" condition="cellType=='info'">      <text label="Text" type="TextArea" height="50" defaultValue="" placeholder="Enter text here" condition="cellType=='info'"/>      <showIn label="Display In" type="ComboBox" options="Cell,Lightbox" data="cell,lightbox" defaultValue="cell" conditionTrigger="true" condition="cellType=='info'" tooltip="Determines where the information is shown when the cell is selected"/>      <replace label="Add Text" type="ComboBox" options="At end,Replace cell content" data="end,replace" defaultValue="end" condition="cellType=='info'&amp;&amp;showIn=='cell'"/>      <required label="Required" type="CheckBox" defaultValue="false" condition="cellType=='info'" tooltip="Require this information to be viewed before moving on to the next stage &amp;/or receive feedback"/>    </information>    <question label="Question" type="group" tooltip="Display a text field or drop down menu in the table cell" condition="cellType=='question'">      <answer label="Answer" type="TextInput" defaultValue="" placeholder="Enter answer here" condition="cellType=='question'" tooltip="Where there is more than possible correct answer, list each one separated by a comma"/>      <type label="Type" options="Text field,Drop down menu" type="ComboBox" data="text,menu" defaultValue="text" condition="cellType=='question'" tooltip="When set to drop down menu, use slashes to separate answer options, e.g. England/Scotland/Wales where the first option is the correct answer"/>      <qShowIn label="Display In" type="ComboBox" options="Cell,Lightbox" data="cell,lightbox" defaultValue="cell" conditionTrigger="true" condition="cellType=='question'" tooltip="Determines where the question is shown"/>      <qText label="Text" type="TextArea" height="50" defaultValue="" placeholder="Enter text here" condition="cellType=='question'&amp;&amp;qShowIn=='lightbox'"/>"    </question>    <feedback label="Feedback" type="TextArea" height="50" defaultValue="" optional="true" condition="cellType=='question'&amp;&amp;qShowIn=='lightbox'"/>  </tableCell></wizard>