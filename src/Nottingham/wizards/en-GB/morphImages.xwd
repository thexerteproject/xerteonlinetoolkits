<wizard menus="Media">  <pageWizard remove="true" duplicate="false">    <newNodes>      <morphImages><![CDATA[<morphImages name="Enter Page Title" text="Enter Page Text" url="Select Image" tip1="Enter a Description for Accessibility" url2="Select Image" tip2="Enter a Description for Accessibility" zoomSpeed="3" buttonLabel="Zoom"><hotspot name="Zoom Region" x="0" y="0" w="100" h="100" /></morphImages>]]></morphImages>    </newNodes>  </pageWizard>  <!-- MORPH IMAGES========================== -->  <morphImages menu="Media" menuItem="Morph Images" hint="Morph between two images. Can be used to present an impression of zooming into a region of an image." icon="icZoomIn" thumb="thumbs/morphImages.jpg" example="https://xot.xerte.org.uk/play_116?hide=bottom#morphimages" remove="true">    <name label="Page Title" type="TextInput" wysiwyg="true"/>    <text label="Page Text" type="TextArea" height="250"/>		<image1 type="group" label="Image 1">		<url label="File" type="media"/>		<tip1 label="Description" type="TextInput"/>	</image1>		<image2 type="group" label="Image 2">		<url2 label="File" type="media"/>		<tip2 label="Description" type="TextInput"/>	</image2>	    <zoomSpeed label="Zoom Speed" type="NumericStepper" min="1" max="10" step="1" defaultValue="3"/>	    <buttonLabel label="Button Label" type="TextInput" wysiwyg="true" divider="true" language="true"/>    <zoomBtnTip label="Button Tip" type="textInput" language="true" deprecated="This property is no longer required" />	    <zoomBtnWidth label="Button Width" type="NumericStepper" min="80" max="250" step="10" width="100" defaultValue="100" language="true" deprecated="Width is now set automatically" />  </morphImages></wizard>