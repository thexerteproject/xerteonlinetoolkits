<wizard menus="Links / Feeds">  <pageWizard remove="true" duplicate="false">    <newNodes>      <rssdownload><![CDATA[<rssdownload name="Enter Page Title" text="Enter text for the page here" url="Enter Feed URL" align="Left" panelWidth="Medium"/>]]></rssdownload>    </newNodes>  </pageWizard>  <rssdownload menuItem="RSS Xerte Download Feed" menu="Links / Feeds" hint="Include Xerte specific RSS feeds from any version 3.8 or later Xerte installation to include download links for any projects that have been included in the RSS export feed." icon="icRSS" thumb="thumbs/rssdownload.jpg" example="https://xot.xerte.org.uk/play_116?hide=bottom#rssxertedownloadfeed" remove="true">    <name label="Page Title" type="TextInput" wysiwyg="true"/>    <text label="Page Text" type="textArea" height="300"/>	<align label="Text Align" options="Left,Right" type="ComboBox" data="Left,Right" defaultValue="Left" width="100"/>    <url label="Feed URL" type="textInput"/>	<panelWidth label="Panel Width" options="Small,Medium,Large,Full" type="ComboBox"  data="Small,Medium,Large,Full" defaultValue="Medium" width="100" mandatory="true"/>		<appearance label="Feed View" options="Title Only,Title &amp; Description" type="ComboBox" data="title,snippet" defaultValue="snippet" width="100" optional="true"/>	<rssDLLinks type="group" label="Download links &amp; info" optional="true" tooltip="By default each of the download links are shown and some info elements are hidden. Show/Hide items via these options e.g. tick to show, untick to hide.">    		<deployment label="Deployment" type="CheckBox" defaultValue="true" optional="true"/>    		<scorm12 label="SCORM 1.2 (XOT Projects)" type="CheckBox" defaultValue="true" optional="true"/>    		<scorm2004 label="SCORM 2004 (XOT Projects)" type="CheckBox" defaultValue="true" optional="true"/>    		<author label="Project author" type="CheckBox" defaultValue="false" optional="true"/>    		<license label="License" type="CheckBox" defaultValue="false" optional="true"/>    		<moddate label="Last modified date" type="CheckBox" defaultValue="false" optional="true"/>    		<feedheader label="RSS feed header" type="CheckBox" defaultValue="false" optional="true"/>    	</rssDLLinks>  </rssdownload></wizard>