<wizard menus="Media">  <pageWizard remove="true" duplicate="false">    <newNodes>      <jmol><![CDATA[<jmol name="Enter Page Title" text="Enter text for the page" xyz="Select an XYZ file" size="Medium" initLook="Ball and Stick" colour="Default" controls="true"/>]]></jmol>    </newNodes>  </pageWizard>  <!-- JMOL VIewer================================= -->  <jmol menu="Media" menuItem="JMOL Viewer" hint="Provides a viewer for JMOL files" icon="icAtom" thumb="thumbs/jmol.jpg" remove="true">    <name label="Page Title" type="TextInput"/>    <text label="Text" type="TextArea" height="150"/>    <xyz label="Model File" type="Media"/>    <size type="ComboBox" options="Small,Medium,Large" label="Model Size" defaultData="Medium"  width="100"/>    <initLook label="Initial Appearance" options="CPK,Ball and Stick,Sticks,Wireframe,Cartoon,Backbone" type="ComboBox" data="CPK,Ball and Stick,Sticks,Wireframe,Cartoon,Backbone" defaultData="Ball and Stick"  width="120"/>    <colour type="ComboBox" options="Default,Element,Amino Acid,Chain,Secondary Structure" label="Initial Colour" defaultValue="Default" width="140"/>      <controls label="Show Default Controls" type="CheckBox" defaultValue="true"/>        <narrationNavigate label="Navigate on narration" type="CheckBox" defaultValue="false" optional="true"/>    <playNarration label="Auto-play narration" type="CheckBox" defaultValue="false" optional="true"/>    <narration label="Narration" type="media"  optional="true"/>    <navSetting label="Navigation Buttons" options="All,Back and Next,Back Only,Next Only,None" type="ComboBox" width="120" data="all,backnext,backonly,nextonly,none" defaultValue="all" optional="true"/>    <pageID label="Page ID" defaultValue="Unique ID for this page" type="TextInput" optional="true" />    <cartoonOpt label="Cartoon Option" type="CheckBox" defaultValue="true" optional="true"/>    <backOpt label="Backbone Option" type="CheckBox" defaultValue="true" optional="true"/>    <colourOpt type="CheckBox" label="Control Bar - Colour" optional="true"/>    <labelOpt type="CheckBox" label="Control Bar - Label" optional="true"/>    <surfaceOpt type="CheckBox" label="Control Bar - Surface" optional="true"/>    <scriptOpt type="CheckBox" label="Control Bar - Script" optional="true"/>    <initScript type="TextInput" label="Initial Jmol Script" optional="true"/>    <bgColour label="Background Colour" type="ComboBox" options="White,Black,Blue" width="120" optional="true"/>    <initSpin label="Spin" type="CheckBox" optional="true"/>    <xyz2 label="2nd Model File" type="Media" optional="true"/>    	<newNodes>      <jmolOption><![CDATA[<jmolOption name="Enter option label here" script="Enter Jmol script here"/>]]></jmolOption>    </newNodes>  </jmol>    <jmolOption menuItem="Script Option" icon="icBullet" remove="true">  	<name label="Label" type="TextInput"/>	<script label="Jmol Script" type="TextArea" height="100"/>    </jmolOption></wizard>