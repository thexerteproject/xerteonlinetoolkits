<wizard menus="Navigators">  <pageWizard remove="true" duplicate="false">    <newNodes>      <list><![CDATA[<list name="Enter Page Title"      panelWidth="Large" panelWidthCustom="50" align="right" panelStyle="false"      openFirst="true"/>]]></list>    </newNodes>  </pageWizard>  <!-- INTERACTIVE LIST================================= -->  <list menu="Navigators" menuItem="Interactive List" hint="Present an interactive list where each list item presents further information." icon="icList" thumb="thumbs/interactiveList.jpg" example="https://xot.xerte.org.uk/play_116?hide=bottom#interactivelist" wiki="https://xerte.org.uk/wiki/doku.php?id=navigator_interactivelist_page" remove="true" deepLink="true">    <name label="Page Title" type="TextInput" wysiwyg="true"/>    <layoutGroup label="Page Layout" type="group">        <panelWidth label="Panel Width" options="Small,Medium,Large,Custom" type="ComboBox" data="XSmall,Small,Large,Medium" defaultValue="Large" conditionTrigger="true"/>        <panelWidthCustom label="Custom Width (%)" type="NumericStepper" min="10" max="90" step="1" defaultValue="70" condition="panelWidth=='Medium'"/>        <panelHeight label="Panel Height" options="Fill Space,Fit Content" type="ComboBox"  data="fill,fit" defaultValue="fill"/>        <align label="Panel Position" options="Right,Left" type="ComboBox" data="right,left" defaultValue="Right"/>        <panelStyle label="Remove Panel Style" type="CheckBox" defaultValue="false"/>    </layoutGroup>    <openFirst label="Show First Item" type="Checkbox" defaultValue="false" mandatory="true" conditionTrigger="true" tooltip="Show first item when page is first viewed"/>    <text label="Text Above Panel" type="TextArea" height="100" optional="true"/>    <textBottom label="Text Below Panel" type="TextArea" height="100" optional="true"/>    <textBehind label="Text Behind Panel" type="TextArea" height="200" optional="true" condition="openFirst!='true'"/>		<btnColours type="group" label="List Buttons" optional="true">		<hicol label="Text Colour" type="ColourPicker" optional="true" defaultValue="0x000000" tooltip="The colour of the text"/>		<hicolback label="Background Colour" type="ColourPicker" optional="true" defaultValue="0xEEEEEE" tooltip="The background colour of the button"/>		<hicolHover label="Hover Background Colour" type="ColourPicker" optional="true" defaultValue="0xCCCCCC" tooltip="The colour of the button when the mouse is hovering over the button"/>	</btnColours>    <newNodes>      <listItem><![CDATA[<listItem name="Enter Item Title" text=""/>]]></listItem>    </newNodes>  </list>  <listItem menuItem="List Item" icon="icBullet" remove="true">    <name label="Title" type="TextInput" wysiwyg="true" />    <text label="Text" type="TextArea" height="250" placeholder="Enter text for the list item here"/>    <align label="Align Text" options="Top,Bottom" type="ComboBox" data="Top,Bottom" defaultValue="Top" width="100" deprecated="This property is deprecated."/>		<imgInfo type="group" label="Image" optional="true">		<url label="Image" type="media"  optional="true" defaultValue="" placeholder="Select an image"/>		<tip label="Image Description" type="textInput" optional="true" defaultValue="" placeholder="Enter a description for accessibility"/>        <caption label="Caption" type="textInput" optional="true" defaultValue="" wysiwyg="true" placeholder="Enter the caption text" tooltip="Optional: add a caption below the image"/>	</imgInfo>	  </listItem>  </wizard>