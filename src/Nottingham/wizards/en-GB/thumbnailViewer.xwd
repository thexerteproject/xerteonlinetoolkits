<wizard menus="Media">  <pageWizard remove="true" duplicate="false">    <newNodes>      <thumbnailViewer><![CDATA[<thumbnailViewer name="Enter Page Title" text="" nextBtnTip="Scroll Forward" priorBtnTip="Scroll Back"/>]]></thumbnailViewer>    </newNodes>  </pageWizard>  <!-- THUMBNAIL VIEWER======================= -->  <thumbnailViewer menu="Media" menuItem="Thumbnail Viewer" hint="A series of thumbnail images for the user to browse." icon="icPictures" thumb="thumbs/thumb.jpg" example="https://xot.xerte.org.uk/play_116?hide=bottom#thumbnailviewer" wiki="https://xerte.org.uk/wiki/doku.php?id=media_thumbnail_viewer_page" remove="true" deepLink="true">        <name label="Page Title" type="TextInput" wysiwyg="true"/>    <text label="Page Text" type="TextArea" height="250" placeholder="Enter text for the page"/>	    <nextBtnTip label="Scroll Forward Tip" type="textInput" language="true" />    <priorBtnTip label="Scroll Back Tip" type="textInput" language="true" />    <QFGroup type="group" lightbox="form" subgroup="assistant" label="Quick Fill" tooltip ="Generate a blank template based on your specifications." optional="true">      <qfThumbnailImage label="Number of Thumbnails" type="TextInput" wysiwyg="true" defaultValue = "5" optional="true"/>      <qfButton type="quickfillbutton" optional="true"/>    </QFGroup>    <newNodes>      <thumbnailImage><![CDATA[<thumbnailImage name="Image" text="" align="Top" url="" linktarget="none" link="" tip=""/>]]></thumbnailImage>    </newNodes>  </thumbnailViewer>    <thumbnailImage menuItem="Image" icon="icBullet" remove="true">    <name label="Label" type="TextInput" wysiwyg="true" />    <text label="Image Text" type="TextArea" height="250" placeholder="Enter text here" />		<image type="group" label="Image">		<url label="File" type="media" placeholder="Select an image"/>		<tip label="Description" type="TextInput" mandatory="true" defaultValue="" placeholder="Enter a description for accessibility" />	</image>		<linkGroup label="Link" type="group">		<linktarget label="Link from image" conditionTrigger="true" options="None,Original image,Specified URL" type="ComboBox"  data="None,Original image,Specified URL" defaultValue="None" width="200"/>		<link label="URL" type="textInput" condition="linktarget=='Specified URL'" placeholder="Enter a URL"/>	</linkGroup>  </thumbnailImage>  </wizard>