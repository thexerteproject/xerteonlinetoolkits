<wizard menus="Navigators">  <pageWizard remove="true" duplicate="false">    <newNodes>      <thumbnailViewer><![CDATA[<thumbnailViewer name="Enter Page Title" text="Enter text for the page" nextBtnTip="Next" priorBtnTip="Previous"/>]]></thumbnailViewer>    </newNodes>  </pageWizard>  <!-- THUMBNAIL VIEWER======================= -->  <thumbnailViewer menu="Navigators" menuItem="Thumbnail Viewer" hint="Presents a series of thumbnail images for the user to browse." icon="icPictures" thumb="thumbs/thumb.jpg" remove="true">        <name label="Page Title" type="TextInput" wysiwyg="true"/>    <text label="Text" type="TextArea" height="250"/>	    <nextBtnTip label="Next Button Tip" type="textInput" divider="true" language="true" />	    <priorBtnTip label="Prior Button Tip" type="textInput" language="true" />	    <narrationNavigate label="Navigate on Narration" type="CheckBox" defaultValue="false" optional="true"/>    <playNarration label="Auto-Play Narration" type="CheckBox" defaultValue="false" optional="true"/>    <narration label="Narration" type="media" optional="true"/>    <navSetting label="Navigation Buttons" options="All,Back and Next,Back Only,Next Only,None" type="ComboBox" width="120" data="all,backnext,backonly,nextonly,none" defaultValue="all" optional="true"/>    <pageID label="Page ID" defaultValue="Unique ID for this page" type="TextInput" optional="true" />    <newNodes>      <thumbnailImage><![CDATA[<thumbnailImage name="name" text="Content for the pane" align="Top" url="" linktarget="none" link=""/>]]></thumbnailImage>    </newNodes>  </thumbnailViewer>    <thumbnailImage menuItem="Nested Page" icon="icBullet" remove="true">    <name label="Page Title" type="TextInput" wysiwyg="true" />    <text label="Page Text" type="TextArea" height="250" />	<linktarget label="Link from image" options="None,Original image,Specified URL" type="ComboBox"  data="None,Original image,Specified URL" defaultValue="None" width="200"/>    <link label="Specified URL" type="textInput"/>	      <url label="Image" type="media"/>    <tip label="Image Description" type="textInput"  optional="true" defaltValue="Enter a Description for Accessibility"/>  </thumbnailImage>  </wizard>