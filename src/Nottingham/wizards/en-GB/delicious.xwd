<wizard menus="Misc">
  <pageWizard remove="true" duplicate="false">
    <newNodes>
      <delicious><![CDATA[<delicious name="Enter Page Title" text="Enter text for the page here" search="recent" term="Enter Tags" results="20" panelWidth="Medium" 
	  headerTxt="Delicious bookmarks for" />]]></delicious>
    </newNodes>
  </pageWizard>

  <!-- DELICIOUS============================ -->
  <delicious menu="Misc" menuItem="Delicious Bookmarks" hint="Include Delicious bookmarks in your project." thumb="thumbs/delicious.jpg" icon="icDelicious" remove="true">

    <name label="Page Title" type="TextInput" wysiwyg="true"/>
    <text label="Instructions" type="textArea" height="300"/>
    <term label="Search Term" type="textInput"/>
    <search label="Search in" options="Recent,Popular,User" type="ComboBox" data="recent,popular,user" defaultValue="recent" width="100"/>
    <results label="Number of Results" type="NumericStepper" min="5" max="100" step="1" defaultValue="20"/>
	<panelWidth label="Panel Width" options="Small,Medium,Large" type="ComboBox"  data="Small,Medium,Large" defaultValue="Medium" width="100" mandatory="true"/>
	
	<headerTxt label="Results Text" type="TextInput" wysiwyg="true" divider="true" language="true" />

  </delicious>


</wizard>