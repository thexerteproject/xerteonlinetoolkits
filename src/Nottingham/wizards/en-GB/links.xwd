<wizard menus="Navigators">  <pageWizard remove="true" duplicate="false">    <newNodes>      <links><![CDATA[<links name="Enter Page Title" text="Enter Page Text" cols="3" height="80"/>]]></links>    </newNodes>  </pageWizard>  <!-- LINKS PAGE================================= -->  <links menu="Navigators" menuItem="Hyperlinks" hint="The links page allows you to easily assemble a page of keyboard accessible links" icon="icWorld" remove="true" thumb="thumbs/links.jpg">        <name label="Page Title" type="TextInput"/>    <text label="Page Title" type="TextArea" height="100"/>    <cols label="Number of Columns" type="NumericStepper" min="1" max="5" step="1" defaultValue="3"/>    <height label="Link Height" options="Small,Medium,Large" type="ComboBox" data="55,80,105" defaultValue="80" width="100"/>    <narrationNavigate label="Navigate on narration" type="CheckBox" defaultValue="false" optional="true"/>    <playNarration label="Auto-play narration" type="CheckBox" defaultValue="false" optional="true"/>    <narration label="Narration" type="media"  optional="true"/>    <navSetting label="Navigation Buttons" options="All,Back and Next,Back Only,Next Only,None" type="ComboBox" width="120" data="all,backnext,backonly,nextonly,none" defaultValue="all" optional="true"/>   <pageID label="Page ID" defaultValue="Unique ID for this page" type="TextInput" optional="true" />    <newNodes>      <link><![CDATA[<link name="Link title" text="Link Description" url="Link URL"/>]]></link>    </newNodes>  </links>  <link icon="icTagGreen">    <name label="Link Title" type="TextInput"/>    <text label="Link Description" type="TextArea" height="100"/>    <url label="URL" type="TextInput"/>  </link></wizard>