<wizard menus="Links / Feeds">  <pageWizard remove="true" duplicate="false">    <newNodes>      <links><![CDATA[<links name="Enter Page Title" text="Enter Page Text" cols="3" />]]></links>    </newNodes>  </pageWizard>  <!-- LINKS PAGE================================= -->  <links menu="Links / Feeds" menuItem="Hyperlinks" hint="A list of keyboard accessible links." icon="icWorld" remove="true" thumb="thumbs/links.jpg" example="https://xot.xerte.org.uk/play_116?hide=bottom#hyperlinks">        <name label="Page Title" type="TextInput" wysiwyg="true"/>    <text label="Page Text" type="TextArea" height="100"/>    <cols label="Number of Columns" type="NumericStepper" min="1" max="5" step="1" defaultValue="3"/>    <height label="Link Height" options="Small,Medium,Large" type="ComboBox" data="55,80,105" defaultValue="80" width="100" deprecated="This property is deprecated. Height is now set automatically."/>    <newNodes>      <link><![CDATA[<link name="Link title" text="Link Description" url="Link URL"/>]]></link>    </newNodes>  </links>  <link menuItem="Link" icon="icTagGreen" remove="true">    <name label="Link Title" type="TextInput" wysiwyg="true"/>    <text label="Description" type="TextArea" height="100"/>    <url label="URL" type="TextInput" tooltip="Tip: You can add your link(s) to the text area above and leave this URL field blank. This allows more control over where the links appear as well as control over the specific text and/or images to which the links are attached."/>  </link></wizard>