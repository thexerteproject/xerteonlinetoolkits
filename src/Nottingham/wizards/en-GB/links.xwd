<wizard menus="Links / Feeds">  <pageWizard remove="true" duplicate="false">    <newNodes>      <links><![CDATA[<links name="Enter Page Title" text="" cols="3" />]]></links>    </newNodes>  </pageWizard>  <!-- LINKS PAGE================================= -->  <links menu="Links / Feeds" menuItem="Hyperlinks" hint="A list of keyboard accessible links." icon="icWorld" remove="true" thumb="thumbs/links.jpg" example="https://xot.xerte.org.uk/play_116?hide=bottom#hyperlinks" wiki="https://xerte.org.uk/wiki/doku.php?id=linksfeeds_hyperlinks_page" >        <name label="Page Title" type="TextInput" wysiwyg="true"/>    <text label="Page Text" type="TextArea" height="100" placeholder="Enter text for the page here"/>    <cols label="Number of Columns" type="NumericStepper" min="1" max="5" step="1" defaultValue="3"/>    <height label="Link Height" options="Small,Medium,Large" type="ComboBox" data="55,80,105" defaultValue="80" width="100" deprecated="This property is deprecated. Height is now set automatically."/>    <newNodes>      <link><![CDATA[<link name="Link Title" text="" url="" urlTxt="" target="new"/>]]></link>    </newNodes>  </links>  <link menuItem="Link" icon="icTagGreen" remove="true">    <name label="Link title" type="TextInput" wysiwyg="true"/>    <text label="Description" type="TextArea" height="100" placeholder="Enter a description of the link here"/>    <url label="URL" type="TextInput" placeholder="e.g. https://www.xerte.org.uk" tooltip="Tip: You can add your link(s) to the text area above and leave this URL field blank. This allows more control over where the links appear."/>    <urlTxt label="Link text" type="TextInput" wysiwyg="true" defaultValue="" placeholder="e.g. Xerte Community Website" mandatory="true" tooltip="The text for the link. If left empty, the full URL will be shown on screen. To maximise accessibility, it is best for the link text to be a meaningful description of where the link points."/>	<target label="Open in" options="New Window,Lightbox" type="ComboBox" data="new,lightbox" defaultValue="new" mandatory="true"/>  </link></wizard>