<wizard menus="Media">  <pageWizard remove="true" duplicate="false">    <newNodes>          <videoSynch><![CDATA[<videoSynch name="Enter Page Title" url="Select a Video" text="Enter text for the page here" pause="True" erase="True"/>]]></videoSynch>    </newNodes>  	    </pageWizard>  <!-- SYNCHED VIDEO=============================================== -->  <videoSynch menu="Media" menuItem="Synched Video" hint="Synchronise passages of text to a movie." icon="icPageWhiteOffice"  thumb="thumbs/videoSynch.jpg" example="https://xot.xerte.org.uk/play_116?hide=bottom#synchedvideo" remove="true">    <name label="Page Title" type="TextInput" wysiwyg="true" />    <url label="Video" type="Media" />    <text label="Page Text" type="TextArea"  height="200"/>    <pause label="Pause on Synch Points" options="True,False" type="ComboBox" data="True,False" defaultValue="True"  width="100"/>    <erase label="Erase on Synch Points" options="True,False" type="ComboBox" data="True,False" defaultValue="True"  width="100"/>		<movieSize label="Video Size" type="textInput" defaultValue="320,240" optional="true"/>	<timedText label="Timed Text File" type="Media" optional="true"/>    <newNodes>      <videoSynchPoint><![CDATA[<videoSynchPoint name="Enter Title Here" text="Content for the pane" align="Top" synch="0"/>]]></videoSynchPoint>    </newNodes>  </videoSynch>    <videoSynchPoint menuItem="Synch Point" icon="icBullet" remove="true">    <name label="Title" type="TextInput" wysiwyg="true" />    <text label="Text" type="TextArea" height="250" />    <align label="Align Text" options="Top,Bottom" type="ComboBox" data="Top,Bottom" defaultValue="Top" width="100"/>		<imgInfo type="group" label="Image" optional="true">		<url label="Image" type="media"  optional="true" defaultValue="Select an Image"/>		<tip label="Description" type="textInput"  optional="true" defaultValue="Enter a Description for Accessibility"/>	</imgInfo>		<synch label="Synch Point" type="TextInput"/>      </videoSynchPoint>  </wizard>