<wizard menus="Media">  <pageWizard remove="true" duplicate="false">    <newNodes>          <videoSynch><![CDATA[<videoSynch name="Enter Page Title" url="" text="" pause="True" erase="True"/>]]></videoSynch>    </newNodes>  	    </pageWizard>  <!-- SYNCHED VIDEO=============================================== -->  <videoSynch menu="Media" menuItem="Synched Video" hint="Synchronise passages of text to a movie." icon="icPageWhiteOffice"  thumb="thumbs/videoSynch.jpg" example="https://xot.xerte.org.uk/play_116?hide=bottom#synchedvideo" wiki="https://xerte.org.uk/wiki/doku.php?id=media_synched_video_page" remove="true">    <name label="Page Title" type="TextInput" wysiwyg="true" />    <url label="Video" type="Media" placeholder="Select a video file or enter a URL/embed code" />    <text label="Page Text" type="TextArea"  height="200" placeholder="Enter text for the page here"/>    <pause label="Pause on Synch Points" options="True,False" type="ComboBox" data="True,False" defaultValue="True"  width="100"/>    <erase label="Erase on Synch Points" options="True,False" type="ComboBox" data="True,False" defaultValue="True"  width="100"/>		<movieSize label="Video Size" type="textInput" defaultValue="" placeholder="e.g. 320,240" optional="true" tooltip="By default videos will be scaled automatically to best fit on the page. This option fixes the video size (width,height). Use with caution!"/>	<timedText label="Timed Text File" type="Media" optional="true" placeholder="Select a timed text file"/>	<trackingWeight label="Tracking Score Weight" optional="true" type="textInput" width="120" defaultValue="1.0" tooltip="Customise the weight of the task on this page, relative to other scored pages in the project. Setting the weight to 0 will mean that the task on this page will not contribute to the project's overall score." />    <newNodes>      <videoSynchPoint><![CDATA[<videoSynchPoint name="Enter Title Here" text="Content for the pane" align="Top" synch="0"/>]]></videoSynchPoint>    </newNodes>  </videoSynch>    <videoSynchPoint menuItem="Synch Point" icon="icBullet" remove="true">    <name label="Title" type="TextInput" wysiwyg="true" />    <text label="Text" type="TextArea" height="250" />    <align label="Align Text" options="Top,Bottom" type="ComboBox" data="Top,Bottom" defaultValue="Top" width="100"/>		<imgInfo type="group" label="Image" optional="true">		<url label="Image" type="media"  optional="true" defaultValue="Select an Image"/>		<tip label="Description" type="textInput"  optional="true" defaultValue="" placeholder="Enter a description for accessibility"/>		<caption label="Caption" type="textInput" optional="true" defaultValue="" wysiwyg="true" placeholder="Enter the caption text" tooltip="Optional: add a caption below the image"/>	</imgInfo>		<synch label="Synch Point" type="TextInput"/>      </videoSynchPoint>  </wizard>