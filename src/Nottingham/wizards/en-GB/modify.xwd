<wizard menus="Interactivity">  <pageWizard remove="true" duplicate="false">    <newNodes>      <modify><![CDATA[<modify name="Enter Page Title"  instruction="Provide instructions here" text="Provide initial text here" prompt="Provide a prompt here" answer="Provide a model answer here"	   answerHeaderTxt="Write your answer here:" feedbackBtnTxt="Feedback"	  />]]></modify>    </newNodes>  </pageWizard>  <!-- MODIFY INTERACTION============================== -->  <modify menu="Interactivity" menuItem="Modify Exercise" hint="Present a piece of text that the user must modify in some way, for example, by changing the tense." icon="icModify" thumb="thumbs/modify.jpg" remove="true">        <name label="Page Title" type="TextInput" wysiwyg="true"/>    <instruction label="Instruction" type="TextArea" height="100"/>    <text label="Initial Text" type="script" height="100"/>    <prompt label="Answer Prompt" type="script" height="100"/>    <answer label="Model Answer" type="script" height="100"/>    <answerHeaderTxt label="Instruction Header" type="TextInput" wysiwyg="true" divider="true" language="true" />    <feedbackBtnTxt label="Button Label" type="TextInput" wysiwyg="true" language="true" />    <feedbackBtnTip label="Button Tip" type="textInput" language="true" deprecated="This property is no longer required"/>	    <feedbackBtnWidth label="Button Width" type="NumericStepper" min="80" max="250" step="10" width="100" defaultValue="100" language="true" deprecated="Width is now set automatically"/>		<timer label="Page Timer" defaultValue="60" type="NumericStepper" min="0" max="1200" step="1" width="100" optional="true" />  </modify></wizard>