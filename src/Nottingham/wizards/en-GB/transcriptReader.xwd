<wizard menus="Media">  <pageWizard remove="true" duplicate="false">    <newNodes>          <transcriptReader><![CDATA[<transcriptReader name="Enter Page Title" sound="" text="" panelWidth="Medium"/>]]></transcriptReader>    </newNodes>  	    </pageWizard>  <!-- TRANSCRIPT READER============================================= -->  <transcriptReader menu="Media" menuItem="Transcript Reader" hint="Synchronise a transcript to an audio file." icon="icTranscript2"  thumb="thumbs/transcriptReader.jpg" example="https://xot.xerte.org.uk/play_116?hide=bottom#transcriptreader" wiki="https://xerte.org.uk/wiki/doku.php?id=media_transcript_reader_page" remove="true">    <name label="Page Title" type="TextInput" wysiwyg="true" />    <text label="Page Text" type="TextArea"  height="200" placeholder="Enter text for the page here"/>	<sound label="Sound" type="Media" placeholder="Select an audio file" />    <panelWidth label="Panel Width" options="Small,Medium,Large,Full" type="ComboBox"  data="Small,Medium,Large,Full" defaultValue="Small" width="100"/>		<timedText label="Timed Text File" type="Media" optional="true" placeholder="Select a timed text file"/>    <newNodes>      <nestedSynch><![CDATA[<nestedSynch text="" synch="0" name="Synch Point" />]]></nestedSynch>    </newNodes>  </transcriptReader>  <nestedSynch menuItem="Text" icon="icBullet" remove="true">    <name label="Label" type="TextInput" mandatory="true" defaultValue="Synch point" tooltip="The label only appears in the editor's project structure panel and is not shown in the project itself. Unique and meaningful labels can be used to make synch points easier to locate for editing at a later date."/>    <text label="Text" type="TextArea" height="250" placeholder="Enter text here" />    <synch label="Synch Point" type="TextInput"/>	  </nestedSynch>    </wizard>