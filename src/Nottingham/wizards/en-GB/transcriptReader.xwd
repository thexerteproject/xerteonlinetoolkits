<wizard menus="Media">  <pageWizard remove="true" duplicate="false">    <newNodes>          <transcriptReader><![CDATA[<transcriptReader name="Enter Page Title" sound="Select a Sound File" text="Enter text for the page here" panelWidth="Medium"/>]]></transcriptReader>    </newNodes>  	    </pageWizard>  <!-- TRANSCRIPT READER============================================= -->  <transcriptReader menu="Media" menuItem="Transcript Reader" hint="Synchronise a transcript to an audio file." icon="icTranscript2"  thumb="thumbs/transcriptReader.jpg" example="https://xot.xerte.org.uk/play_116?hide=bottom#transcriptreader" wiki="https://xerte.org.uk/wiki/doku.php?id=media_transcript_reader_page" remove="true">    <name label="Page Title" type="TextInput" wysiwyg="true" />    <text label="Page Text" type="TextArea"  height="200"/>	<sound label="Sound" type="Media" />    <panelWidth label="Panel Width" options="Small,Medium,Large,Full" type="ComboBox"  data="Small,Medium,Large,Full" defaultValue="Small" width="100"/>		<timedText label="Timed Text File" type="Media" optional="true"/>    <newNodes>      <nestedSynch><![CDATA[<nestedSynch text="Enter text here" synch="0" name="Synch point" />]]></nestedSynch>    </newNodes>  </transcriptReader>  <nestedSynch menuItem="Text" icon="icBullet" remove="true">    <name label="Label" type="TextInput" mandatory="true" defaultValue="Synch point" tooltip="The label only appears in the editor's project structure panel and is not shown in the project itself. Unique and meaningful labels can be used to make synch points easier to locate for editing at a later date."/>    <text label="Text" type="TextArea" height="250" />    <synch label="Synch Point" type="TextInput"/>	  </nestedSynch>    </wizard>