<wizard menus="Interactivity">  <pageWizard remove="true" duplicate="false">    <newNodes>      <timeline><![CDATA[<timeline name="Enter Page Title" text="" interactivity="Timeline" feedback="" checkBtnTxt="Check Answers" allWrong="You have not placed any labels correctly. Try again." allWrongTracking="You have not placed any labels correctly." score="You have scored {i} out of {n}. Try again." scoreTracking="You have scored {i} out of {n}." incomplete="Complete the exercise." />]]></timeline>    </newNodes>  </pageWizard>  <!-- TIMELINE INTERACTION========================= -->  <timeline menu="Interactivity" menuItem="Timeline / Matching Pairs" hint="An interaction for creating either a timeline or matching activity with labels to drag and drop in place." icon="icTime" thumb="thumbs/timeline.jpg" example="https://xot.xerte.org.uk/play_116?hide=bottom#timelinematchingpairs" wiki="https://xerte.org.uk/wiki/doku.php?id=interactivity_timeline_page" remove="true">    <name label="Page Title" type="TextInput" wysiwyg="true"/>    <text label="Page Text" type="TextArea" height="250" placeholder="Enter text for the page here"/>    <interactivity label="Interactivity" options="Timeline,Matching Pairs" type="ComboBox" data="Timeline,Matching Pairs" defaultValue="Timeline" width="120"/>    <feedback label="Feedback" type="TextArea" height="100" placeholder="Enter feedback here"/>    <checkBtnTxt label="Check Button Label" type="TextInput" wysiwyg="true" divider="true" language="true" />    <checkBtnTip label="Check Button Tip" type="textInput" language="true" deprecated="This property is no longer required"/>	    <checkBtnWidth label="Check Button Width" type="NumericStepper" min="100" max="250" step="10" width="100" defaultValue="100" language="true" deprecated="Width is now set automatically"/>	<allWrong label="All Wrong Text" type="TextInput" wysiwyg="true" language="true"/>	<score label="{i} out of {n} Text" type="TextInput" wysiwyg="true" language="true"/>	<allWrongTracking label="All Wrong Text Tracking" type="TextInput" wysiwyg="true" language="true"/>	<scoreTracking label="{i} out of {n} Text Tracking" type="TextInput" wysiwyg="true" language="true"/>	<incomplete label="Incomplete Text" type="TextInput" wysiwyg="true" language="true"/>	<trackingWeight label="Tracking Score Weight" optional="true" type="textInput" width="120" defaultValue="1.0" tooltip="Customise the weight of the task on this page, relative to other scored pages in the project. Setting the weight to 0 will mean that the task on this page will not contribute to the project's overall score." />    <newNodes>      <timelinedate><![CDATA[<timelinedate name="Enter Item Name" text=""/>]]></timelinedate>    </newNodes>  </timeline>  <timelinedate menuItem="Item" icon="icPageWhite" max="12" remove="true">    <name label="Name" type="TextInput" wysiwyg="true"/>    <text label="Description" type="TextArea" height="100" placeholder="Enter description for item here"/>  </timelinedate></wizard>