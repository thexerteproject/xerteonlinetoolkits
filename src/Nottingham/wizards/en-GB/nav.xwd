<wizard menus="Navigators">  <pageWizard remove="true" duplicate="false">    <newNodes>          <nav><![CDATA[<nav name="Enter Page Title" text="" type="Tab" nextBtnTip="Next" prevBtnTip="Previous" slideCount="Slide {i} of {n}"/>]]></nav>    </newNodes>  	    </pageWizard>    <!-- NAVIGATOR=================================================== -->  <nav menu="Navigators" menuItem="Multi-Navigator" hint="Present related text and graphical content on a series of sub-pages. Layout options include accordion, button, columns, slideshow and tabs." icon="icPages" thumb="thumbs/tabNav.jpg" example="https://xot.xerte.org.uk/play_116?hide=bottom#multi-navigator" wiki="https://xerte.org.uk/wiki/doku.php?id=navigator_multinavigator_page" remove="true" deepLink="true">	    <name label="Page Title" type="TextInput" wysiwyg="true" />    <text label="Page Text" type="TextArea" height="250" placeholder="Enter text for the page here" />	<type label="Navigator Type" options="Accordion,Buttons,Columns,Slideshow,Tabs" type="ComboBox" conditionTrigger="true" data="Acc,Button,Col,Slide,Tab" width="100"/>		<nextBtnTip label="Slideshow Button Tip (Next)" type="TextInput" language="true"/>    <prevBtnTip label="Slideshow Button Tip (Previous)" type="TextInput" language="true"/>    <slideCount label="Slide {i} of {n}" type="TextInput" wysiwyg="true" language="true" />    	<panelInfo type="group" label="Panel Layout" optional="true">		<panelWidth label="Panel Width" options="Small,Medium,Large,Full" type="ComboBox"  data="Small,Medium,Large,Full" defaultValue="Medium" width="100" optional="true" condition="type!='Col'"/>		<panelHeight label="Panel Height" options="Fill Space,Fit Content" type="ComboBox"  data="fill,fit" defaultValue="fill" width="100" optional="true" condition="type!='Col'"/>		<panelPos label="Panel Position" options="Left,Right" type="ComboBox" data="Left,Right" defaultValue="Right" width="100" optional="true" condition="type!='Col'"/>		<buttonPos label="Button Position" options="Top,Bottom" type="ComboBox" data="Top,Bottom" defaultValue="Top" width="100" optional="true" condition="type=='Button'||type=='Slide'"/>		<collapsible label="Collapse Accordion" type="CheckBox" defaultValue="false" optional="true" condition="type=='Acc'"/>	</panelInfo>    <QFGroup type="group" label="Quick Fill" tooltip ="Generate a blank template based on your specifications." optional="true">       <qfNavPage label="Number of Columns" type="TextInput" wysiwyg="true" defaultValue = "3" optional="true"/>       <qfButton type="quickfillbutton" optional="true"/>    </QFGroup>    <AIGroup type="group" label="AI" tooltip ="Generate content automatically with the help of AI." optional="true">        <aiSelector label="AI model" options="GPT (OpenAI),Claude (Anthropic), Mistral AI" type="comboBox" data="openai,anthropic,mistralai" defaultValue="openai" width="120" conditionTrigger="true" optional="true"/>        <subjects label="Page Subjects" type="TextInput" tooltip="List the subject for each page separated by a comma (,). tab subjects may be single words or sentences (as long as they don't contain commas). Examples: 'Mammals, birds, fish', 'birds endemic to the Netherlands, common birds found in Europe, birds found exclusively outside of Europe'." defaultValue = "" placeholder="Enter page subjects here. E.g. Subject 1, Subject 2..." optional="true"/>        <additionalInstructions label="Instructions" type = "text" tooltip="Specify the kind of content which should be generated in each page of the tab. For example: 'A list of examples', 'fun facts', 'definitions', 'questions'. If needed, provide a more elaborate description. For example, that each column provides a summary of its subject based on the uploaded file. Pages can later be fine-tuned, or new content generated." wysiwyg="true" defaultValue = "Each page should contain a brief summary of its respective subject." optional="true"/>        <exampleContent label="Example Content (Optional)" type = "text" tooltip = "Optionally, provide an example of the kind of content you expect as an output. The model will then attempt to generate a similar output, or follow the additional instructions on how to handle or extrapolate on this output. For example, you may provide a sentence structure which the model will then attempt to emulate for the other subjects." wysiwyg="true" optional="true"/>        <useContext label="Generate Based on Context" type="checkbox" defaultValue="false" tooltip="When selected, the AI model will also take into account the current contents of the entire learning object when generating this object." condition="aiSelector=='openai'" optional ="true"/>        <ageRange label="Reading Level" type="TextInput" tooltip="The estimated age, grade, education or other qualifier for which the AI will attempt to curate the complexity of the language. Examples: students aged 12-16, grade 7 students in the Netherlands, university graduates, office workers in the education field, A2 level Dutch speakers, etc." defaultValue="" placeholder="Enter reading level. Defaults to 'Intermediate English, B2'" optional="true"/>        <eduLevel label="Education Level" type="TextInput" tooltip="The education level for which the content is intended. Examples: University students, HBO students in the Netherlands, Middle school students who already have learned the basics, etc."  defaultValue="" placeholder="Enter educational level. Defaults to 'University.'" optional="true"/>        <voiceSelector label="Tone and Style" type="TextInput" tooltip = "Adjusts the tone and style of the generated content. Possibilities include: formal, semi-formal, informal, semi-informal, active, passive as well as combinations. Additional modifiers are possible, but may result in less consistent content. Examples: 'Semi-formal teaching, active voice', 'formal, passive voice, usage instructions'." defaultValue="" placeholder="Enter an indicator of tone. Defaults to 'Semi-formal.'" optional="true"/>        <uploadPrompt label="Information Source" options="None (uses model base knowledge), Submit Snippet, Upload File" type="comboBox" data="false,trueText,true" defaultValue="false" tooltip="Select 'Submit Snippet' when you wish for the model to use the provided text as a source, 'Upload File' when you wish to upload a file or when using a video or audio link as a source. Please ensure the uploaded file or provided snippet corresponds to the requested subject. If relevant information is not found in the snippet or file, the AI will attempt to supplement with its own knowledge." conditionTrigger="true" condition="aiSelector=='openai'" optional="true"/>        <textSnippet label="Text Snippet" type="text" tooltip="Provide the text snippet you wish the model to use as a source for generating the interactivity." condition="uploadPrompt=='trueText'" optional="true"/>        <file label="File" type="media" defaultValue="Upload a file or enter a video link here..." tooltip="Select a video, audio, powerpoint, PDF or other text file to be used for content generation." condition="uploadPrompt=='true'" optional="true"/>        <fileAccessPrompt label="Allow information supplementation" type="checkbox" defaultValue="false" tooltip = "Instruct the AI to use information beyond what was provided in the file, using its own knowledge stores. Keep in mind that this may result in inaccurate information and should be thoroughly double-checked!" condition="uploadPrompt=='true'" optional ="true"/>        <aiButton type="aibutton" optional="true"/>        <explanation label="This content is generated using AI. Make sure to double-check and adjust!" type="info" optional="true"/>   </AIGroup>    <newNodes>      <navPage><![CDATA[<navPage name="Enter Title Here" text=""/>]]></navPage>    </newNodes>	  </nav>  <navPage menuItem="Nested Page" icon="icBullet" remove="true">    <name label="Title" type="TextInput" wysiwyg="true" />    <text label="Text" type="TextArea" height="250" placeholder="Enter text for the nested page here" />    <AIGroup type="group" label="AI" optional="true">         <aiSelector label="AI model" options="GPT (OpenAI),Claude (Anthropic), Mistral AI" type="comboBox" data="openai,anthropic,mistralai" defaultValue="openai" width="120" conditionTrigger="true" optional="true"/>         <subject label="Subject" type="TextInput" defaultValue="" placeholder="Enter a subject here..." tooltip="This determines which area or subject the generated words will follow." optional="true"/>         <useContext label="Generate Based on Context" type="checkbox" defaultValue="false" tooltip="When selected, the AI model will also take into account the current contents of the learning object when generating this object." condition="aiSelector=='openai'" optional="true"/>         <additionalInstructions label="Additional Instructions" type="text" tooltip="Provide any additional instructions here. For example: create a summary, generate content in the form of a list, use only chapter 3 from the uploaded file. It is also possible to ask questions. For example: 1. Write a summary of the final results of the experiment 2. Specify how they tested their findings. Finally, additional instructions can be targeted and interact with example content. For example, you can copy an interesting paragraph or point into the example content, and then ask to: make the example content more concise, expand on the example content, add more specific examples to the content, add points which are only relevant for teachers in the Netherlands/Flanders, and so on." wysiwyg="true" optional="true" defaultValue="Provide a brief summary on the given subject."/>         <exampleContent label="Example Content (Optional)" type="text" tooltip="Optionally, provide an example of the kind of content you expect as an output. The model will then attempt to generate a similar output, or follow the additional instructions on how to handle or extrapolate on this output." wysiwyg="true" optional="true"/>         <ageRange label="Reading Level" type="TextInput" tooltip="The estimated age, grade, education or other qualifier for which the AI will attempt to curate the complexity of the language. Examples: students aged 12-16, grade 7 students in the Netherlands, university graduates, office workers in the education field, A2 level Dutch speakers, etc." defaultValue="" placeholder="Enter reading level. Defaults to 'Intermediate English, B2'" optional="true"/>         <eduLevel label="Education Level" type="TextInput" tooltip="The education level for which the content is intended. Examples: University students, HBO students in the Netherlands, Middle school students who already have learned the basics, etc." defaultValue="" placeholder="Enter educational level. Defaults to 'University.'" optional="true"/>         <voiceSelector label="Tone and Style" type="TextInput" tooltip = "Adjusts the tone and style of the generated content. Possibilities include: formal, semi-formal, informal, semi-informal, active, passive as well as combinations. Additional modifiers are possible, but may result in less consistent content. Examples: 'Semi-formal teaching, active voice', 'formal, passive voice, usage instructions'." defaultValue="" placeholder="Enter an indicator of tone. Defaults to 'Semi-formal.'" optional="true"/>         <uploadPrompt label="Information Source" options="None (uses model base knowledge), Submit Snippet, Upload File" type="comboBox" data="false,trueText,true" defaultValue="false" tooltip="Select 'Submit Snippet' when you wish for the model to use the provided text as a source, 'Upload File' when you wish to upload a file or when using a video or audio link as a source. Please ensure the uploaded file or provided snippet corresponds to the requested subject. If relevant information is not found in the snippet or file, the AI will attempt to supplement with its own knowledge." conditionTrigger="true" condition="aiSelector=='openai'" optional="true"/>         <textSnippet label="Text Snippet" type="text" tooltip="Provide the text snippet you wish the model to use as a source for generating the interactivity." condition="uploadPrompt=='trueText'" optional="true"/>         <file label="File" type="media" defaultValue="Upload a file or enter a video link here..." tooltip="Select a video, audio, powerpoint, PDF or other text file to be used for content generation." condition="uploadPrompt=='true'" optional="true"/>         <fileAccessPrompt label="Allow information supplementation" type="checkbox" defaultValue="false" tooltip = "Instruct the AI to use information beyond what was provided in the file, using its own knowledge stores. Keep in mind that this may result in inaccurate information and should be thoroughly double-checked!" condition="uploadPrompt=='true'" optional ="true"/>         <aiButton type="aibutton" optional="true"/>         <explanation label="This content is generated using AI. Make sure to double-check and adjust!" type="info" optional="true"/>    </AIGroup>	<mediaInfo type="group" label="Media" optional="true">		<url label="File" type="media" defaultValue="" placeholder="Select a media file" optional="true" conditionTrigger="true"/>		<tip label="Description" type="TextInput" defaultValue="" placeholder="Enter a description for accessibility" optional="true"/>		<align label="Media Position" options="Top,Bottom" type="ComboBox" data="Top,Bottom" defaultValue="Top" width="100" optional="true"/>	</mediaInfo>	<transcript label="Audio Transcript" type="textArea" height="100" defaultValue="" condition="url!=undefined" optional="true" tooltip="For use when the media added is an audio file: add a transcript button to the audio player to reveal this text"/>    <caption label="Image Caption" type="textInput" optional="true" defaultValue="" wysiwyg="true" condition="url!=undefined" placeholder="Enter the caption text" tooltip="For use when the media added is an image file: add a caption below the image"/>  </navPage></wizard>