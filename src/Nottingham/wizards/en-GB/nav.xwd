<wizard menus="Navigators">  <pageWizard remove="true" duplicate="false">    <newNodes>          <nav><![CDATA[<nav name="Enter Page Title" text="" type="Tab"      panelWidth="Medium" panelWidthCustom="50" panelHeight="fill" panelPos="Right" buttonPos="Top" collapsible="false" panelStyle="false" removeTxt="true"      showTitles="true" minWidth="75" columnPanelStyle="none"      nextBtnTip="Next" prevBtnTip="Previous" slideCount="Slide {i} of {n}"/>]]></nav>    </newNodes>  	    </pageWizard>    <!-- NAVIGATOR=================================================== -->  <nav menu="Navigators" menuItem="Multi-Navigator" hint="Present related text and graphical content on a series of sub-pages. Layout options include accordion, button, columns, slideshow and tabs." icon="icPages" thumb="thumbs/tabNav.jpg" example="https://xot.xerte.org.uk/play_116?hide=bottom#multi-navigator" wiki="https://xerte.org.uk/wiki/doku.php?id=navigator_multinavigator_page" remove="true" deepLink="true">	    <name label="Page Title" type="TextInput" wysiwyg="true" />    <text label="Page Text" type="TextArea" height="250" placeholder="Enter text for the page here" condition="panelWidth!='Full'||(removeTxt!='true'&amp;&amp;removeTxt!=undefined)"/>	<type label="Navigator Type" options="Accordion,Buttons,Columns,Slideshow,Tabs" type="ComboBox" conditionTrigger="true" data="Acc,Button,Col,Slide,Tab" width="100"/>    <layoutGroup label="Page Layout" type="group">        <panelWidth label="Panel Width" options="Small,Medium,Large,Full,Custom" type="ComboBox" data="Small,Medium,Large,Full,Custom" defaultValue="Medium" conditionTrigger="true" mandatory="true" condition="type!='Col'"/>        <panelWidthCustom label="Custom Width (%)" type="NumericStepper" min="10" max="90" step="1" defaultValue="50" mandatory="true" condition="type!='Col'&amp;&amp;panelWidth=='Custom'"/>        <panelHeight label="Panel Height" options="Fill Space,Fit Content" type="ComboBox"  data="fill,fit" defaultValue="fill" width="100" mandatory="true" condition="type!='Col'"/>        <panelPos label="Panel Position" options="Right,Left" type="ComboBox" data="Right,Left" defaultValue="Right" mandatory="true" condition="type!='Col'&amp;&amp;panelWidth!='Full'"/>        <buttonPos label="Button Position" options="Top,Bottom" type="ComboBox" data="Top,Bottom" defaultValue="Top" width="100" mandatory="true" condition="type=='Button'||type=='Slide'"/>        <collapsible label="Collapse Accordion" type="CheckBox" defaultValue="false" mandatory="true" condition="type=='Acc'"/>        <panelStyle label="Remove Panel Style" type="CheckBox" defaultValue="false" mandatory="true" condition="type!='Col'"/>        <removeTxt label="Remove Page Text" type="CheckBox" defaultValue="true" conditionTrigger="true" mandatory="true" condition="type!='Col'&amp;&amp;panelWidth=='Full'"/>        <showTitles label="Column Titles" type="CheckBox" defaultValue="true" mandatory="true" condition="type=='Col'"/>        <minWidth label="Minimum Column Width (px)" type="NumericStepper" min="20" max="1000" defaultValue="75" mandatory="true" condition="type=='Col'" tooltip="Columns will never be narrower than this width and will be wrapped onto a new line when required"/>        <columnPanelStyle label="Panel" type="ComboBox" options="None,Single panel,Each column" data="none,single,each" defaultValue="none" condition="type=='Col'" mandatory="true"/>    </layoutGroup>    <textTopGroup label="Text Above Navigator" type="group" optional="true">        <textTop label="Text" type="TextArea" height="100" optional="true"/>        <textTopWidth label="Full Width" type="CheckBox" optional="true" defaultValue="false" condition="panelWidth!='Full'&amp;&amp;type!='Col'"/>    </textTopGroup>    <textBelowGroup label="Text Below Navigator" type="group" optional="true">        <textBottom label="Text" type="TextArea" height="100" optional="true"/>        <textBottomWidth label="Full Width" type="CheckBox" optional="true" defaultValue="false" condition="panelWidth!='Full'&amp;&amp;type!='Col'"/>    </textBelowGroup>    <nextBtnTip label="Slideshow Button Tip (Next)" type="TextInput" language="true"/>    <prevBtnTip label="Slideshow Button Tip (Previous)" type="TextInput" language="true"/>    <slideCount label="Slide {i} of {n}" type="TextInput" wysiwyg="true" language="true" />	    <newNodes>      <navPage><![CDATA[<navPage name="Enter Title Here" text=""/>]]></navPage>    </newNodes>	  </nav>  <navPage menuItem="Nested Page" icon="icBullet" remove="true">    <name label="Title" type="TextInput" wysiwyg="true" />    <text label="Text" type="TextArea" height="250" placeholder="Enter text for the nested page here" />		<mediaInfo type="group" label="Media" optional="true">		<url label="File" type="media" defaultValue="" placeholder="Select a media file" optional="true" conditionTrigger="true"/>		<tip label="Description" type="TextInput" defaultValue="" placeholder="Enter a description for accessibility" optional="true"/>		<align label="Media Position" options="Top,Bottom" type="ComboBox" data="Top,Bottom" defaultValue="Top" width="100" optional="true"/>	</mediaInfo>	<transcript label="Audio Transcript" type="textArea" height="100" defaultValue="" condition="url!=undefined" optional="true" tooltip="For use when the media added is an audio file: add a transcript button to the audio player to reveal this text"/>    <caption label="Image Caption" type="textInput" optional="true" defaultValue="" wysiwyg="true" condition="url!=undefined" placeholder="Enter the caption text" tooltip="For use when the media added is an image file: add a caption below the image"/>  </navPage> </wizard>