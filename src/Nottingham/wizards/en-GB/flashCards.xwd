<wizard menus="Navigators">  <pageWizard remove="true" duplicate="false">    <newNodes>      <flashCards><![CDATA[<flashCards name="Enter Page Title" text="Enter text for the page here" cardNoTxt="Card {i} of {n}" flipTxt="Flip Card" nextTxt="Next" prevTxt="Previous" backScrollTip="Scroll Back" nextScrollTip="Scroll Forward"	  />]]></flashCards>          </newNodes>  </pageWizard>  <!-- FLASH CARDS=============================== -->    <flashCards menu="Navigators" menuItem="Flash Cards" icon="icRefreshPage" remove="true" thumb="thumbs/flashCards.jpg" example="https://xot.xerte.org.uk/play_116?hide=bottom#flashcards" hint="A series of double sided flash cards containing text and media.">	    <name label="Page Title" type="TextInput" wysiwyg="true" />    <text label="Page Text" type="TextArea" height="120"/>	<!--<align label="Text Align" type="ComboBox" options="Top,Bottom" data="top,bottom" defaultValue="top" width="100" mandatory="true"/>-->	<random label="Randomise Cards" type="Checkbox" optional="true" tooltip="Randomise the order of the cards" />    <amountToShow label="Cards To Show" type="NumericStepper" min="1" max="1000" step="1" defaultValue="1" optional="true" tooltip="Use with 'Randomise Cards' to show a limited number of randomly selected cards"/>	<displayStyle label="Card Layout" type="ComboBox" options="One at a time,In a row: Horizontal,In a row: Vertical" data="one,horizontal,vertical" optional="true" defaultValue="one" tooltip="Display cards one at a time or in a row"/>	<sizing type="group" label="Card Size" optional="true">		<cardSize label="Width" type="ComboBox" options="Small,Medium,Large,Full" data="small,medium,large,full" defaultValue="medium" width="100" optional="true"/>		<cardHeight label="Height" type="ComboBox" options="Fill Space,Fit to Content,Match Tallest Card" data="default,fit,match" defaultValue="default" width="100" optional="true"/>	</sizing>		<cardNoTxt label="Card No. Text" type="TextInput" language="true" />    <flipTxt label="Flip Button Label" type="TextInput" language="true"/>	<nextTxt label="Next Button Label" type="TextInput" language="true" />	<prevTxt label="Previous Button Label" type="TextInput" language="true" />	<backScrollTip label="Scroll Back Label" type="TextInput" wysiwyg="true" language="true" />    <nextScrollTip label="Scroll Forward Label" type="TextInput" wysiwyg="true" language="true" />	    <newNodes>      <card><![CDATA[<card name="Enter Title Here" side1="Content for side one of the card" side2="Content for side two of the card"/>]]></card>    </newNodes>	  </flashCards>    <card menuItem="Card" icon="icTwoPages" remove="true">	<name label="Title" type="TextInput" wysiwyg="true"/>	<side1 label="Side 1 Text" type="TextArea" height="120"/>	<side2 label="Side 2 Text" type="TextArea" height="120"/>		<side1Media type="group" label="Side 1 Media" optional="true" tooltip="Media (e.g. image) shown on the front of this card">		<media1 label="File" type="Media" optional="true" defaultValue="Select a File"/>		<tip1 label="Description" type="textInput" optional="true" defaultValue="Enter a Description for Accessibility"/>		<align1 label="Position" options="Top,Bottom" type="ComboBox" optional="true" data="Top,Bottom" defaultValue="Bottom" width="100"/>	</side1Media>		<side2Media type="group" label="Side 2 Media" optional="true" tooltip="Media (e.g. image) shown on the back of this card">		<media2 label="File" type="Media" optional="true" defaultValue="Select a File"/>		<tip2 label="Description" type="textInput" optional="true" defaultValue="Enter a Description for Accessibility"/>		<align2 label="Position" options="Top,Bottom" type="ComboBox" optional="true" data="Top,Bottom" defaultValue="Bottom" width="100"/>	</side2Media>	  </card></wizard>