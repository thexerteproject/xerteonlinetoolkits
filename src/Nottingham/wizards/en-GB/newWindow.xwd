<wizard menus="Links / Feeds">  <pageWizard remove="true" duplicate="false">    <newNodes>      <newWindow><![CDATA[<newWindow name="Enter Page Title" text="" url="" descrip="" target="new" previewImage="" tip="" windowWidth="550" windowHeight="400"/>]]></newWindow>    </newNodes>  </pageWizard>  <!-- NEW WINDOW=================== -->  <newWindow menu="Links / Feeds" menuItem="New Window" hint="Allow the user to launch a new window to a URL." icon="icApplication" thumb="thumbs/newWindow.jpg" example="https://xot.xerte.org.uk/play_116?hide=bottom#newwindow" wiki="https://xerte.org.uk/wiki/doku.php?id=linksfeeds_newwindow_page" remove="true">    <name label="Page Title" type="TextInput" wysiwyg="true"/>    <text label="Page Text" type="TextArea" height="150" placeholder="Enter text for the page"/>		<link type="group" label="Link">		<url label="URL" type="TextInput" placeholder="e.g. https://xerte.org.uk"/>		<descrip label="Text" type="TextInput" tooltip="The text for the link. If left empty, the full URL will be shown on screen. To maximise accessibility, it is best for the link text to be a meaningful description of where the link points."/>	</link>		<target label="Open In" options="New Window,Lightbox" type="ComboBox" data="new,lightbox" defaultValue="new" mandatory="true"/>		<image type="group" label="Preview Image">		<previewImage label="File" type="media" placeholder="Select an image"/>		<tip label="Description" type="TextInput" placeholder="Enter a description for accessibility"/>	</image>		<window type="group" label="Window">		<windowWidth label="Width" type="NumericStepper" min="100" max="980" step="10" defaultValue="550" width="100"/>		<windowHeight label="Height" type="NumericStepper" min="100" max="720" step="10" defaultValue="400" width="100"/>	</window>    <caption label="Caption" type="textInput" optional="true" defaultValue="" wysiwyg="true" placeholder="Enter the caption text" tooltip="Add a caption below the image"/>    <htm label="HTML" type="textArea" height="150" optional="true" tooltip="Create your own content to open in the new window"/>  </newWindow></wizard>