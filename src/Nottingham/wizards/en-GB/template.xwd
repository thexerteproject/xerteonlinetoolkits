<?xml version="1.0"?>
<wizard menus="Text,Media,Navigators,Connectors,Charts,Interactivity,Games,Links / Feeds,Tracking / xAPI">

  <learningObject icon="icPackage" remove="false" duplicate="false">

    <name type="TextInput" wysiwyg="true" label="Project Title"/>
    <navigation type="ComboBox" options="Linear,Historic,Menu,Menu with page controls,Linear with historic back button" data="Linear,Historic,Menu,Menu with Page Controls,LinearWithHistoric" label="Navigation" width="170" conditionTrigger="true"/>
    <language type="LanguageList" label="Language" width="170"/>
    <theme type="ThemeList" label="Theme" mandatory="true" conditionTrigger="true" defaultValue="default"/>
	<themeIcons type="CheckBox" label="Update Theme Icons" mandatory="true" defaultValue="false" condition="theme_list[currtheme].imgbtns=='true'||theme==undefined||theme=='default'" tooltip="The selected theme uses images for the navigation and footer tool buttons. Most newer themes use Font Awesome icons instead, which give you greater flexibility in their appearance. Checking this box will automatically update interface buttons to use Font Awesome icons. It is recommended that you check this if you use are using any of the Customise Button Icon options so that all buttons appear consistent."/>
	<displayMode type="ComboBox" options="Fixed size,Fill window" data="default,fill window" label="Display Mode" width="100" conditionTrigger="true" tooltip="By default, the project will fill the browser window. When display mode is set to 'fixed size', the project will be limited to 800x600 pixels. This fixed size can be changed using the 'fixed display size' optional property."/>
	<responsive type="CheckBox" label="Responsive Text" defaultValue="false" mandatory="true" conditionTrigger="true"/>
	<textSize type="ComboBox" options="10,12,14,16,18,24,36" label="Default Text Size" width="100" tooltip="The default text size will only be used when responsive text is turned off. It can also be overridden when a text size is directly set in the text editor" condition="responsive!='true'"/>
	
	<!-- OPTIONAL PROPERTIES -->
	
	<maxWidth optional="true" type="NumericStepper" min="800" max="2000" step="10" label="Max Width" defaultValue="1400" tooltip="Use this option to set the maximum width (in pixels) that the page contents will be displayed on larger displays"/>
	<fixDisplay optional="true" type="TextInput" label="Fixed Display Size" defaultValue="800,600" condition="displayMode=='default'||(fixDisplay!=undefined&amp;&amp;fixDisplay!='')" tooltip="Sets the project dimensions (in pixels) when 'display mode' is set to 'fixed size'"/>
	
	<fullScreenGroup type="group" label="Full Screen Button" optional="true" condition="displayMode=='default'||(fixDisplay!=undefined&amp;&amp;fixDisplay!='')" tooltip="Customise the tooltip descriptions and icons used on the full screen toggle button">
		<fullScreenIcons type="CheckBox" label="Customise Button Icons" defaultValue="false" conditionTrigger="true" condition="displayMode=='default'||(fixDisplay!=undefined&amp;&amp;fixDisplay!='')" optional="true" tooltip="Override the default icons used on the full screen button. When checked, you will see further options allowing you to select the icons used. When left unchecked, the images/icons will be those specified by the theme."/>
		<maxScreenGroup type="group" label="Full Screen" optional="true">
			<maxLabel label="Tooltip" type="TextInput" defaultValue="Full Screen" condition="displayMode=='default'||(fixDisplay!=undefined&amp;&amp;fixDisplay!='')" optional="true"/>
			<maxIcon label="Icon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-expand-arrows-alt" condition="(displayMode=='default'||(fixDisplay!=undefined&amp;&amp;fixDisplay!=''))&amp;&amp;fullScreenIcons=='true'" optional="true"/>
		</maxScreenGroup>
		<minScreenGroup type="group" label="Fixed Size" optional="true">
			<minLabel label="Tooltip" type="TextInput" defaultValue="Default" condition="displayMode=='default'||(fixDisplay!=undefined&amp;&amp;fixDisplay!='')" optional="true"/>
			<minIcon label="Icon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-compress-arrows-alt" condition="(displayMode=='default'||(fixDisplay!=undefined&amp;&amp;fixDisplay!=''))&amp;&amp;fullScreenIcons=='true'" optional="true"/>
		</minScreenGroup>
	</fullScreenGroup>
	
	<hideHeader optional="true" type="CheckBox" label="Hide Header" defaultValue="true" tooltip="Hide the header bar containing project &amp; page titles"/>
	<hideFooter optional="true" type="CheckBox" label="Hide Footer" defaultValue="true" tooltip="Hide the footer bar containing the navigation buttons"/>
	
	<navGroup type="group" label="Navigation Buttons" optional="true" tooltip="Customise the tooltip descriptions and icons used on the navigation buttons">
		<navIcons type="CheckBox" label="Customise Button Icons" defaultValue="false" conditionTrigger="true" optional="true" tooltip="Override the default icons used on the navigation buttons. When checked, you will see further options allowing you to select the icons used. When left unchecked, the images/icons will be those specified by the theme."/>
		<prevGroup type="group" label="Previous Page" optional="true">
			<prevLabel label="Tooltip" type="TextInput" defaultValue="Back" condition="navigation!='Menu'" optional="true"/>
			<prevIcon label="Icon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-chevron-circle-left" condition="navIcons=='true'&amp;&amp;navigation!='Menu'" optional="true"/>
		</prevGroup>
		<nextGroup type="group" label="Next Page" optional="true">
			<nextLabel label="Tooltip" type="TextInput" defaultValue="Next" condition="navigation!='Menu'" optional="true"/>
			<nextIcon label="Icon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-chevron-circle-right" condition="navIcons=='true'&amp;&amp;navigation!='Menu'" optional="true"/>
		</nextGroup>
		<tocGroup type="group" label="Table of Contents" optional="true">
			<tocLabel label="Tooltip" type="TextInput" defaultValue="Contents" condition="navigation!='Historic'" optional="true" tooltip="The tooltip description for the Table of Contents button. This text also appears as the title of the dialog box that opens when the button is clicked (unless navigation is set to menu or menu with page controls)."/>
			<tocIcon label="Icon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-bars" condition="navIcons=='true'&amp;&amp;navigation!='Historic'" optional="true"/>
		</tocGroup>
		<homeGroup type="group" label="Home Page" optional="true">
			<homeLabel label="Tooltip" type="TextInput" defaultValue="Home" condition="navigation=='Historic'" optional="true"/>
			<homeIcon label="Icon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-home" condition="navIcons=='true'&amp;&amp;navigation=='Historic'" optional="true"/>
		</homeGroup>
	</navGroup>
	
	<toc type="group" label="Table of Contents" optional="true" tooltip="Customise the appearance of the table of contents / page menu">
		<tocNumbers optional="true" type="CheckBox" label="Page Numbering" defaultValue="true" condition="navigation!='Historic'" tooltip="Number pages shown on the table of contents / page menu"/>
		<pageTick optional="true" type="CheckBox" label="Tick When Viewed" defaultValue="true" condition="navigation!='Historic'" tooltip="Tick pages on the table of contents / page menu when they have been viewed"/>
		<tocTarget label="Display In" type="ComboBox" data="dialog,lightbox" options="Dialog,Lightbox" defaultValue="dialog" condition="navigation=='Linear'||navigation=='LinearWithHistoric'||navigation==undefined" optional="true" tooltip="Determines where the table of contents opens"/>
		<menuImageGroup type="group" label="Menu Image" optional="true" condition="navigation=='Menu'||navigation=='Menu with Page Controls'" tooltip="Add an image to the menu page">
			<menuImage optional="true" type="media" label="Image" defaultValue="Select an image" condition="navigation=='Menu'||navigation=='Menu with Page Controls'"/>
			<menuImageAlt label="Description" type="TextInput" defaultValue="" placeholder="Enter a description for accessibility" condition="(navigation=='Menu'||navigation=='Menu with Page Controls')&amp;&amp;menuImageProperties=='Use Panel'" optional="true"/>
			<menuImageProperties optional="true" type="ComboBox" label="Position" conditionTrigger="true" options="Panel,Background" data="Use Panel,Full Screen" width="100" defaultValue="Use Panel" condition="navigation=='Menu'||navigation=='Menu with Page Controls'"/>
		</menuImageGroup>
	</toc>
	
	<homePageGroup type="group" label="Home Page" optional="true" tooltip="Customise which page is classed as the home page and create a link from the project title to the home page" info="The default home page is either the first page or the page menu (when navigation is menu or menu with page controls).">
		<homePageLink optional="true" type="CheckBox" label="Link Project Title to Home Page" defaultValue="false" conditionTrigger="true" tooltip="Make the project title a link which returns users to the home page"/>
		<homePage optional="true" type="pagelist" label="Home Page" defaultValue="" condition="(navigation=='Historic'||homePageLink!='false')&amp;&amp;navigation!='Menu'&amp;&amp;navigation!='Menu with Page Controls'" tooltip="Select which page is classed as the home page (linked to from project title or home button when navigation is set to 'Historic'). The first page will be the home page if no page selected here."/>
	</homePageGroup>
	
	<progressBar optional="true" type="ComboBox" options="Show progress bar,Show progress bar and hide page counter" data="pBar,pBarNoCounter" label="Progress Bar" defaultValue="pBar" width="100" tooltip="Add a progress bar (indicating the percentage completed) to the footer bar"/>
	
	<footerToolsGroup type="group" label="Footer Tools" optional="true" tooltip="The footer tools are the buttons that normally appear on the bottom left of your project, such as Assessibility Options and Glossary. These options allow you to set the initial visibility state of these buttons and customise the show/hide footer tools button.">
		<footerTools type="ComboBox" label="Default State" options="Hidden,Visible,Visible (Remove show/hide button)" data="hideFooterTools,showFooterTools,none" conditionTrigger="true" defaultValue="showFooterTools" width="100" optional="true"/>
		<footerToolIcons type="CheckBox" label="Customise Button Icons" defaultValue="false" conditionTrigger="true" condition="footerTools!='none'" optional="true" tooltip="Override the default icons used on the show / hide footer tools button. When checked, you will see further options allowing you to select the icons used. When left unchecked, the images/icons will be those specified by the theme."/>
		<hideToolsGroup type="group" label="Hide Footer Tools Button" optional="true">
			<hideToolsLabel label="Tooltip" type="TextInput" defaultValue="Hide footer tools" condition="footerTools!='none'" optional="true"/>
			<hideToolsIcon label="Icon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-angle-double-left" condition="footerTools!='none'&amp;&amp;footerToolIcons=='true'" optional="true"/>
		</hideToolsGroup>
		<showToolsGroup type="group" label="Show Footer Tools Button" optional="true">
			<showToolsLabel label="Tooltip" type="TextInput" defaultValue="Show footer tools" condition="footerTools!='none'" optional="true"/>
			<showToolsIcon label="Icon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-angle-double-right" condition="footerTools!='none'&amp;&amp;footerToolIcons=='true'" optional="true"/>
		</showToolsGroup>
	</footerToolsGroup>
	
	<accessibilityGroup type="group" label="Accessibility" optional="true" tooltip="Options related to the Accessibility Options button">
		<accessibilityLabel label="Tooltip" type="TextInput" defaultValue="Accessibility Options" condition="accessibilityHide!='true'" optional="true" tooltip="The tooltip description for the Accessibility Options button. This text also appears as the title of the dialog box that opens when the button is clicked."/>
		<accessibilityTarget label="Display In" type="ComboBox" data="dialog,lightbox" options="Dialog,Lightbox" defaultValue="dialog" condition="accessibilityHide!='true'" optional="true" tooltip="Determines where the accessibility options information opens"/>
		<accessibilityIc type="CheckBox" label="Customise Button Icon" defaultValue="false" conditionTrigger="true" condition="accessibilityHide!='true'" optional="true" tooltip="Override the default icon used on the Accessibility Options button. When checked, you will see a further option allowing you to select the icon used. When left unchecked, the image/icon will be that specified by the theme."/>
		<accessibilityIcon label="Icon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-eye-slash" condition="accessibilityHide!='true'&amp;&amp;accessibilityIc=='true'" optional="true"/>
		<wcagGroup type="group" label="Logo &amp; Link" optional="true" tooltip="The Accessibility Options dialog contains a WCAG logo which links to the Xerte Accessibility statement">
			<wcagHide label="Hide Logo &amp; Link" type="Checkbox" conditionTrigger="true" defaultValue="false" condition="accessibilityHide!='true'" optional="true"/>
			<wcagAlt label="Logo Description" type="TextInput" defaultValue="WCAG WAI-AA logo" condition="accessibilityHide!='true'&amp;&amp;wcagHide=='false'" optional="true"/>
			<wcagLinkTitle label="Link Tooltip" type="TextInput" defaultValue="View the Xerte accessibility statement" condition="accessibilityHide!='true'&amp;&amp;wcagHide=='false'" optional="true"/>
			<wcagLink type="Media" label="Accessibility Statement Link" tooltip="Add a URL or browse to a file to link to your own accessibility statement. Leave empty to link to the accessibility statement on xerte.org.uk." condition="accessibilityHide!='true'&amp;&amp;wcagHide=='false'" optional="true"/>
		</wcagGroup>
		<accessibilityHide type="CheckBox" label="Hide Button" defaultValue="false" conditionTrigger="true" optional="true" tooltip="Use this option with caution! Although you can hide the Accessibility Options button from the project interface, think carefully before you do this. The accessibility options allow learners to easily change the look of the project to suit their needs and links to information on the accessibility of Xerte."/>
	</accessibilityGroup>
	
	<helpGroup type="group" label="Help File" optional="true" tooltip="Add a Help button to the footer tools, which opens a file or URL when clicked">
		<nfo label="File" type="media" optional="true" placeholder="Select a file (e.g. PDF) or enter a URL"/>
		<helpTarget type="ComboBox" label="Open in" defaultValue="new" options="New window,Lightbox" data="new,lightbox" tooltip="Where the help file will open - by default it will open in a new browser tab/window" optional="true"/>
		<helpLabel label="Tooltip" type="TextInput" defaultValue="Help" optional="true" tooltip="The tooltip description for the Help button"/>
		<helpIc type="CheckBox" label="Customise Button Icon" defaultValue="false" conditionTrigger="true" optional="true" tooltip="Override the default icon used on the Help File button. When checked, you will see a further option allowing you to select the icon used. When left unchecked, the image/icon will be that specified by the theme."/>
		<helpIcon label="Icon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-question" condition="helpIc=='true'" optional="true"/>
	</helpGroup>
	
	<introGroup type="group" label="Project Introduction" optional="true" tooltip="Add a Project Introduction button to the footer tools, which opens information in a lightbox when clicked. This introduction can be set to open automatically when the project first loads.">
		<introType type="ComboBox" label="Content Type" defaultValue="text" options="Text,Image,Video,URL,File" data="text,image,video,url,file" conditionTrigger="true" optional="true" tooltip="The type of content used as an introduction for this project"/>
		<intro label="Text" type="TextArea" height="100" defaultValue="" condition="introType=='text'||introType==undefined" optional="true"/>
		<introImg label="Image" type="media" height="100" defaultValue="" condition="introType=='image'" optional="true" placeholder="Select an image"/>
		<introVideo label="Video" type="media" height="100" defaultValue="" condition="introType=='video'" optional="true" placeholder="Select a video file or enter a URL/embed code"/>
		<introTip label="Description" type="TextInput" defaultValue="" placeholder="Enter a description for accessibility" condition="introType=='image'||introType=='video'" optional="true"/>
		<introCaption label="Caption" type="TextInput" defaultValue="" wysiwyg="true" placeholder="Enter the caption text" condition="introType=='image'" optional="true" tooltip="Optional: add a caption below the image"/>
		<introURL label="URL" type="TextInput" defaultValue="" condition="introType=='url'" optional="true" placeholder="Enter a URL"/>
		<introFile label="File" type="media" height="100" defaultValue="" condition="introType=='file'" optional="true" placeholder="Select a file (e.g. PDF)"/>
		<introTitle type="CheckBox" label="Include Title" defaultValue="true" condition="introType=='text'||introType==undefined||introType=='image'" optional="true" tooltip="Include the project title above the introduction"/>
		<introBtn type="CheckBox" label="Start Button" defaultValue="true" conditionTrigger="true" condition="introType=='text'||introType==undefined||introType=='image'" optional="true" tooltip="Include a 'start' button below the introduction that closes the lightbox when clicked"/>
		<introBtnTxt label="Start Button Text" type="TextInput" defaultValue="Get Started" condition="introBtn=='true'&amp;&amp;(introType=='text'||introType==undefined||introType=='image')" wysiwyg="true" optional="true"/>
		<introWidth label="Lightbox Width" options="Auto,Small,Medium,Large,Full" type="ComboBox" data="Auto,Small,Medium,Large,Full" defaultValue="Auto" condition="introType=='text'||introType==undefined" optional="true"/>
		<introShow type="ComboBox" label="Auto-Open" defaultValue="first" options="When project loads,When project loads on first page,Never" data="always,first,never" optional="true" tooltip="Specify whether the introduction will automatically open when the project first loads"/>
		<introBtnGroup type="group" label="Button" optional="true">
			<introLabel label="Tooltip" type="TextInput" defaultValue="Introduction" tooltip="The tooltip description for the Project Introduction button" optional="true"/>
			<introIc type="CheckBox" label="Customise Button Icon" defaultValue="false" conditionTrigger="true" optional="true" tooltip="Override the default icon used on the Project Introduction button. When checked, you will see a further option allowing you to select the icon used. When left unchecked, the image/icon will be that specified by the theme."/>
			<introIcon label="Icon" type="FontAwesome" iconList="hotspot" condition="introIc!='false'" defaultValue="fas fa-info" optional="true"/>
		</introBtnGroup>
	</introGroup>
	
	<pageIntroGroup type="group" label="Page Introduction Button" optional="true" tooltip="An extra button will appear in the interface if you have added an introduction to any of your pages. Use these properties to control the location and appearance of this button.">
		<pageIntroBg label="Appearance" type="ComboBox" options="Icon only,Circle with icon" data="icon,circle" defaultValue="circle" optional="true"/>
		<pageIntroLabel label="Tooltip" type="TextInput" defaultValue="Page Introduction" optional="true"/>
		<pageIntroIc type="CheckBox" label="Customise Button Icon" defaultValue="false" conditionTrigger="true" optional="true" tooltip="Override the default icon used on the Page Introduction button. When checked, you will see a further option allowing you to select the icon used. When left unchecked, the image/icon will be that specified by the theme."/>
		<pageIntroIcon label="Icon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-info" condition="pageIntroIc=='true'" optional="true"/>
	</pageIntroGroup>
	
	<sideBarGroup type="group" label="Side Bar" optional="true" tooltip="Add a collapsible side bar to the project interface containing buttons which would normally be found on the footer bar.">
		<sideBar type="CheckBox" label="Create Side Bar" defaultValue="true" conditionTrigger="true" optional="true"/>
		<sideBarPosition type="ComboBox" label="Position" defaultValue="left" options="Left,Right" data="left,right" conditionTrigger="true" condition="sideBar!='false'" optional="true"/>
		<sideBarShow type="ComboBox" label="Default State" defaultValue="open" options="Open,Closed" data="open,closed" condition="sideBar!='false'" optional="true"/>
		<sideBarBtns type="group" label="Side Bar Buttons" condition="sideBar!='false'" cols="2" optional="true">
			<sbGlossary label="Glossary" type="Checkbox" defaultValue="true" condition="sideBar!='false'" optional="true"/>
			<sbAccessibility label="Accessibility" type="Checkbox" defaultValue="true" condition="sideBar!='false'" optional="true"/>
			<sbToC label="Table of Contents" type="Checkbox" defaultValue="true" condition="sideBar!='false'" optional="true"/>
			<sbProjectHelp label="Project Help" type="Checkbox" defaultValue="true" condition="sideBar!='false'" optional="true"/>
			<sbProjectIntro label="Project Introduction" type="Checkbox" defaultValue="true" condition="sideBar!='false'" optional="true"/>
			<sbPageIntro label="Page Introduction" type="Checkbox" defaultValue="true" condition="sideBar!='false'" optional="true"/>
		</sideBarBtns>
		<sideBarBtnTxt type="CheckBox" label="Display Button Title" defaultValue="true" condition="sideBar!='false'" optional="true" tooltip="Buttons on the side bar can be shown as either icon only or with visible title text. The title text will match the button tooltip text."/>
		<sideBarSize type="ComboBox" label="Button Size" defaultValue="small" options="Small,Large" data="small,large" condition="sideBar!='false'" optional="true"/>
		<sideBarHideShowGroup type="group" label="Show / Hide Side Bar Buttons" optional="true">
			<sideBarBtnIcons type="CheckBox" label="Customise Button Icons" defaultValue="false" conditionTrigger="true" condition="sideBar!='false'" optional="true" tooltip="Override the default icons used on the show / hide side bar button. When checked, you will see further options allowing you to select the icons used. When left unchecked, the images/icons will be those specified by the theme."/>
			<sideBarHideGroup type="group" label="Hide Side Bar" optional="true">
				<sideBarHideLabel label="Tooltip" type="TextInput" defaultValue="Hide side bar" condition="sideBar!='false'" optional="true"/>
				<sideBarHideLeftIcon label="Icon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-angle-double-left" condition="sideBar!='false'&amp;&amp;sideBarBtnIcons=='true'&amp;&amp;sideBarPosition=='left'" optional="true"/>
				<sideBarHideRightIcon label="Icon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-angle-double-right" condition="sideBar!='false'&amp;&amp;sideBarBtnIcons=='true'&amp;&amp;sideBarPosition=='right'" optional="true"/>
			</sideBarHideGroup>
			<sideBarShowGroup type="group" label="Show Side Bar" optional="true">
				<sideBarShowLabel label="Tooltip" type="TextInput" defaultValue="Show side bar" condition="sideBar!='false'" optional="true"/>
				<sideBarShowLeftIcon label="Icon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-angle-double-right" condition="sideBar!='false'&amp;&amp;sideBarBtnIcons=='true'&amp;&amp;sideBarPosition=='left'" optional="true"/>
				<sideBarShowRightIcon label="Icon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-angle-double-left" condition="sideBar!='false'&amp;&amp;sideBarBtnIcons=='true'&amp;&amp;sideBarPosition=='right'" optional="true"/>
			</sideBarShowGroup>
		</sideBarHideShowGroup>
	</sideBarGroup>
	
	<icon type="group" label="Header Icon/Logo" optional="true" tooltip="Add, hide or customise the logo image shown on the header bar">
		<icHide label="Hide Logo" type="Checkbox" defaultValue="false" optional="true" conditionTrigger="true" tooltip="Hide all default logos automatically added as part of this template/theme"/>
		<ic optional="true" type="media" label="Image" condition="icHide=='false'" defaultValue="" tooltip="Upload an image to use as the header bar logo" placeholder="Select an image"/>
		<icTip optional="true" type="TextInput" label="Description" condition="icHide=='false'" defaultValue="" placeholder="Enter a description for accessibility" tooltip="Enter a description of the logo for accessibility"/>
		<icPosition optional="true" type="ComboBox" options="Left,Right" data="left,right" condition="icHide=='false'" label="Position" defaultValue="left" width="100" tooltip="The position of the logo on the header bar"/>
	</icon>
	
	<bg type="group" label="Background" optional="true" tooltip="Add a background image to the project. This can be overridden by background images added to individual pages.">
		<background optional="true" type="media" label="Image" defaultValue="" placeholder="Select an image"/>
		<backgroundFit optional="true" type="ComboBox" options="Stretch to fit,Crop to fit" data="fill,cover" defaultValue="fill" label="Scaling" tooltip="How the background image fills the available space. 'Stretch to fit' will stretch the image vertically or horizontally to fill the space, while 'Crop to fit' will crop the image to fit without distorting it. Note: this will have no effect in Internet Explorer."/>
		<backgroundopacity optional="true" type="NumericStepper" min="0" max="100" step="1" defaultValue="30" label="Opacity" tooltip="The opacity of the image, where 0 is completely transparent and 100 is not transparent at all"/>
		<backgroundDark optional="true" type="NumericStepper" min="0" max="100" step="1" defaultValue="0" label="Darkness" tooltip="Add a filter to darken the image, where 0 is not darkened at all and 100 is completely darkened (black)"/>
		<backgroundGrey optional="true" type="CheckBox" label="Greyscale" defaultValue="false" tooltip="Add a filter to change the image from colour to greyscale"/>
	</bg>
	
	<stylesheet optional="true" type="Media" label="Stylesheet" tooltip="Upload a CSS file to customise this project" placeholder="Select a CSS file"/>
	<styles optional="true" type="script" height="200" label="Styles" tooltip="Add bespoke CSS to this project"/>
	<script label="Script" type="script" height="200" defaultValue="// JavaScript / jQuery" optional="true" tooltip="Add JavaScript to this project. The code will run after the project interface has been set up but before any pages have loaded."/>
	
	<tracking type="group" label="Tracking" optional="true" tooltip="Properties relating to tracked projects (e.g. using SCORM or xAPI)">
		<trackingMode optional="true" label="Mode" width="250" type="ComboBox" options="Full tracking of first pass,Minimal tracking of first pass,Full tracking of last pass,Minimal tracking of last pass,No tracking" data="full_first,minimal_first,full,minimal,none" defaultValue="full_first"/>
		<trackingPassed optional="true" label="Passing Score" width="250" type="TextInput" defaultValue="55%" tooltip="Passing score as a decimal fraction or percentage" placeholder="e.g. 55%"/>
        <trackingPageTimeout label="Required Time for Page Completion" type="NumericStepper" min="0" max="1000" step="1" optional="true" defaultValue="0" tooltip="The number of seconds a page has to be viewed for to count as completed"/>
        <forceTrackingMode label="Force Tracking Mode Behaviour" type="CheckBox" defaultValue="false" optional="true" tooltip="Force tracking mode behaviour, even in non-tracked projects. This will change the behaviour of some page types, e.g. the restart button will be disabled on Quiz pages and labels will stick on drag and drop exercises, even if they are placed incorrectly"/>
		<restartOptions label="Restart Session" type="ComboBox" options="Restart session if possible,Ask to restart session,Do not restart session" data="restart,ask,do_not_restart" defaultValue="ask" optional="true" tooltip="Restart the session if restart xAPI information is available"/>
		
		<saveSessionGroup type="group" label="Save Session Button" optional="true" tooltip="Options related to the Save Session button">
			<hideSaveSession label="Hide Button" type="CheckBox" defaultValue="false" conditionTrigger="true" optional="true"/>
			<saveSessionLabel label="Tooltip (Save)" type="TextInput" defaultValue="Save Session" condition="hideSaveSession!='true'" optional="true"/>
			<closeSessionLabel label="Tooltip (Close)" type="TextInput" defaultValue="Close Session" condition="hideSaveSession!='true'" optional="true"/>
			<saveSessionIc type="CheckBox" label="Customise Button Icon" defaultValue="false" conditionTrigger="true" condition="hideSaveSession!='true'" optional="true" tooltip="Override the default icon used on the Save Session button. When checked, you will see a further option allowing you to select the icon used. When left unchecked, the image/icon will be that specified by the theme."/>
			<saveSessionIcon label="Icon" type="FontAwesome" iconList="hotspot" defaultValue="fas fa-save" condition="hideSaveSession!='true'&amp;&amp;saveSessionIc=='true'" optional="true"/>
		</saveSessionGroup>
	</tracking>
	
	<meta type="group" label="Metadata" optional="true" tooltip="The metadata for this project used, for example, when shared using oai-pmh">
		<explanation label="Note: to share this project using oai-pmh, you need to give your assent and the fields 'Category' and 'Education level' need to be filled in." type="info" condition="oai_pmh_available" optional="true"/>
		<course label="Course" type="Course" wysiwyg="false" defaultValue="" optional="true" tooltip="The course this project is part of"/>
