<wizard menus="Links / Feeds">  <pageWizard remove="true" duplicate="false">    <newNodes>      <rss><![CDATA[<rss name="Enter Page Title" text="Enter text for the page here" url="Enter Feed URL" align="Left" panelWidth="Medium"/>]]></rss>    </newNodes>  </pageWizard>  <rss menuItem="RSS Feed" menu="Links / Feeds" hint="Include RSS feeds in your project. RSS feeds allow you to add dynamic content from a variety of sources. You can create your own RSS feeds using the RSS Feed template." icon="icRSS" thumb="thumbs/rss.jpg" example="https://xot.xerte.org.uk/play_116?hide=bottom#rssfeed" remove="true">    <name label="Page Title" type="TextInput" wysiwyg="true"/>    <text label="Page Text" type="textArea" height="300"/>	<align label="Text Align" options="Left,Right" type="ComboBox" data="Left,Right" defaultValue="Left" width="100"/>    <url label="Feed URL" type="textInput"/>	<panelWidth label="Panel Width" options="Small,Medium,Large,Full" type="ComboBox"  data="Small,Medium,Large,Full" defaultValue="Medium" width="100" mandatory="true"/>		<appearance label="Feed View" options="Title Only,Title &amp; Snippet" type="ComboBox" data="title,snippet" defaultValue="snippet" width="100" optional="true"/>  </rss></wizard>