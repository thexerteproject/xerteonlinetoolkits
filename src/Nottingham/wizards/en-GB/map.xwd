<wizard menus="Misc">  <pageWizard remove="true" duplicate="false">    <newNodes>      <map><![CDATA[<map name="Enter Page Title" text="Enter text for the page here" align="left" mapurl="Paste the link here" maptype="roadmap" tooltip="Enter a tooltip" link="true"/>]]></map>    </newNodes>  </pageWizard>  <!-- GOOGLE MAP============================ -->  <map icon="icMap" menu="Misc" menuItem="Google Map" hint="Use the map to include an interactive map in the presentation. You can add optional layers of KML data to the map." thumb="thumbs/googleMap.jpg" remove="true" deprecated="This page is no longer supported">    <name label="Page Title" type="TextInput" wysiwyg="true"/>    <text label="Instructions" type="textArea" height="300"/>    <align label="Align Text" options="left,right" type="ComboBox" data="left,right" defaultValue="left" width="100"/>    <mapurl label="Map Link" type="textInput"/>    <maptype label="Map Type" options="roadmap,satellite,terrain,hybrid" type="ComboBox" data="roadmap,satellite,terrain,hybrid" defaultValue="roadmap" width="100"/>    <tooltip label="Map Tooltip" type="TextInput"/>    <link label="Link to Map" type="checkbox" defaultValue="true"/>        <narrationNavigate label="Navigate on narration" type="CheckBox" defaultValue="false" optional="true"/>    <playNarration label="Auto-play narration" type="CheckBox" defaultValue="false" optional="true"/>    <narration label="Narration" type="media"  optional="true"/>    <navSetting label="Navigation Buttons" options="All,Back and Next,Back Only,Next Only,None" type="ComboBox" width="120" data="all,backnext,backonly,nextonly,none" defaultValue="all" optional="true"/>    <pageID label="Page ID" defaultValue="Unique ID for this page" type="TextInput" optional="true" />  </map></wizard>