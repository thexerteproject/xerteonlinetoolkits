<wizard menus="Charts">  <pageWizard remove="true" duplicate="false">    <newNodes>      <table><![CDATA[<table name="Enter Page Title" text="Enter text for the page here" align="left"><tableData name="Table Name" data="data|data" size="medium" borders="simple" header="header" shadeHeader="true" shade="true"/></table>]]></table>    </newNodes>  </pageWizard>  <!-- TABLE PAGE============================ -->  <table menu="Charts" menuItem="Table" hint="A page for basic tables." icon="icTable" remove="true" thumb="thumbs/table.jpg" example="https://xot.xerte.org.uk/play_116?hide=bottom#table" >    <name label="Page Title" type="TextInput" wysiwyg="true"/>    <text label="Page Text" type="TextArea" height="300" width="380"/>    <align label="Align Text" options="Left,Right,Top,Bottom" type="ComboBox" data="left,right,top,bottom" defaultValue="left" width="100"/>    <loadingPrompt label="Loading Message" type="TextInput" wysiwyg="true" divider="true" language="true" deprecated="This property is no longer required"/>  </table>  <tableData menuItem="Table Data" duplicate="false" remove="false" icon="icBullet">    <name label="Table Name" type="TextInput" wysiwyg="true"/>    <data label="Table Data" newRow="data,data" newCol="data" type="DataGrid" height="190" width="390" columns="2" editable="1,1" colWidths="195,195" controls="1" addCols="true" showHeaderRow="false" wysiwyg="full"/>		<appearance type="group" label="Table Appearance">		<size label="Size" options="Small,Medium,Large,Full screen" type="ComboBox" data="small,medium,large,full screen" defaultValue="medium" width="120"/>		<borders label="Borders" options="None,Full grid,Simple,Horizontal only,Vertical only,Outside border" type="ComboBox" data="none,full,simple,horizontal,vertical,outside" defaultValue="simple" width="120"/>		<header label="Header/Footer Rows" options="None,Header,Footer,Both" type="ComboBox" data="none,header,footer,both" defaultValue="header" width="120"/>		<shadeHeader label="Shade Header/Footer" type="Checkbox" defaultValue="true"/>		<shade label="Shade Alternate Rows" type="Checkbox" defaultValue="true"/>	</appearance>    <caption label="Footnote" type="TextInput" wysiwyg="true" optional="true" tooltip="Add a footnote below the table"/>    <alt label="Summary" type="TextInput" wysiwyg="true" defaultValue="Enter a Summary of the Table Contents for Accessibility" optional="true" tooltip="A summary of the table contents that will be read by screenreaders. Add this to improve the accessibility of the page"/>		<csv type="group" label="CSV" optional="true" tooltip="Upload a CSV file of table data. This will override any table data entered directly in the editor">		<url label="File" type="Media" optional="true"/>		<delimiter label="Delimiter" options="Comma,Semicolon" type="ComboBox" data="comma,semicolon" defaultValue="comma" width="100" optional="true"/>	</csv>  </tableData></wizard>