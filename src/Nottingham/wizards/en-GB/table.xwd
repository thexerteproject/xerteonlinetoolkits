<wizard menus="Charts">  <pageWizard remove="true" duplicate="false">    <newNodes>      <table><![CDATA[<table name="Enter Page Title" text="Enter text for the page here" align="left"><tableData name="Table Name" data="data|data" size="medium" borders="simple" header="header" shadeHeader="true" shade="true"/></table>]]></table>    </newNodes>  </pageWizard>  <!-- TABLE PAGE============================ -->  <table menu="Charts" menuItem="Table" hint="A page for basic tables." icon="icTable" remove="true" thumb="thumbs/table.jpg">    <name label="Page Title" type="TextInput" wysiwyg="true"/>    <text label="Text" type="TextArea" height="300" width="380"/>    <align label="Align Text" options="Left,Right,Top,Bottom" type="ComboBox" data="left,right,top,bottom" defaultValue="left" width="100"/>    <loadingPrompt label="Loading Message" type="TextInput" wysiwyg="true" divider="true" language="true" deprecated="This property is no longer required"/>  </table>  <tableData menuItem="Table Data" duplicate="false" remove="false" icon="icBullet">    <name label="Table Name" type="TextInput" wysiwyg="true"/>    <data label="Table Data" newRow="data,data" newCol="data" type="DataGrid" height="190" width="390" columns="2" editable="1,1" colWidths="195,195" controls="1" addCols="true" showHeaderRow="false"/>    <size label="Table Size" options="Small,Medium,Large,Full screen" type="ComboBox" data="small,medium,large,full screen" defaultValue="medium" width="120"/>    <borders label="Borders" options="None,Full grid,Simple,Horizontal only,Vertical only,Outside border" type="ComboBox" data="none,full,simple,horizontal,vertical,outside" defaultValue="simple" width="120"/>    <header label="Header/Footer Rows" options="None,Header,Footer,Both" type="ComboBox" data="none,header,footer,both" defaultValue="header" width="120"/>    <shadeHeader label="Shade Header/Footer" type="Checkbox" defaultValue="true"/>    <shade label="Shade Alternate Rows" type="Checkbox" defaultValue="true"/>    <caption label="Footnote" type="TextInput" wysiwyg="true" optional="true"/>    <alt label="Summary" type="TextInput" wysiwyg="true" defaultValue="Enter a Summary of the Table Contents for Accessibility" optional="true"/>    <url label="CSV File" type="Media" optional="true"/>  </tableData></wizard>