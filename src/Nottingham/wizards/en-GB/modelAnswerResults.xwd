<wizard menus="Interactivity">  <pageWizard remove="true" duplicate="false">    <newNodes>      <modelAnswerResults><![CDATA[<modelAnswerResults name="Enter Page Title" text="" panelWidth="Medium" dataPageLink="true"	  copyBtn2Label="Copy" downloadTxt="Download" pageTxt="Page" notViewed="Page not viewed"	  ></modelAnswerResults>]]></modelAnswerResults>    </newNodes>  </pageWizard>  <!-- Model Answer Results PAGE ================================ -->  <modelAnswerResults menu="Interactivity" menuItem="Model Answer Results" hint="Displays collated results from all viewed Model Answer pages which can then be reviewed and copied or downloaded." icon="icFolderPage" thumb="thumbs/modelAnswerResult.jpg" remove="true" wiki="https://xerte.org.uk/wiki/doku.php?id=interactivity_model_answer_results_page" >    <name label="Page Title" type="TextInput" wysiwyg="true"/>    <text label="Page Text" type="TextArea" height="280" placeholder="Enter text for the page here" mandatory="true" defaultValue=""/>	<panelWidth label="Panel Width" options="Small,Medium,Large" type="ComboBox"  data="Small,Medium,Large" defaultValue="Medium" width="100" mandatory="true"/>    <dataPageLink label="Link to Model Answer Pages" type="CheckBox" condition="dataPageH=='true'||dataPageH==undefined" defaultValue="false" mandatory="true"/>    <info><![CDATA[<p>By default, the results from all viewed Model Answer pages will be displayed on this page. To collate results from a limited number of pages instead, add new 'Page Results' to specify the pages used.</p>]]></info>    <dataGroup label="Customise results" type="group" optional="true" tooltip="Customise the type of data from viewed Model Answer pages that will be shown on this page" info="Customise the data from viewed Model Answer pages that will be shown on this on this page.">      <dataPageGroup label="Page" type="group" optional="true">        <dataPageH label="Title" type="CheckBox" conditionTrigger="true" optional="true" defaultValue="true"/>        <dataPageTxt label="Text" type="CheckBox" optional="true" defaultValue="false"/>        <dataPageFb label="Feedback" type="CheckBox" optional="true" defaultValue="true"/>      </dataPageGroup>      <dataQuestionGroup label="Exercise" type="group" optional="true">        <dataQuestionH label="Titles" type="CheckBox" optional="true" defaultValue="true"/>        <dataQuestionTxt label="Prompt" type="CheckBox" optional="true" defaultValue="true"/>        <dataQuestionAnswer label="Response" type="CheckBox" optional="true" defaultValue="true"/>        <dataQuestionFb label="Feedback" type="CheckBox" optional="true" defaultValue="true"/>      </dataQuestionGroup>    </dataGroup>    <unviewed label="Include unviewed pages" type="CheckBox" defaultValue="true" conditionTrigger="true" optional="true" tooltip="Include data from unviewed Model Answer pages"/>    <copyPaste label="Copy Button" type="CheckBox" defaultValue="true" optional="true" tooltip="Include a button that allows users to easily select &amp; copy their responses"/>    <downloadGroup label="Download Button" type="group" optional="true" tooltip="Include a button that allows users to download a copy their responses">      <showDownload label="Include button" type="CheckBox" optional="true" defaultValue="true" conditionTrigger="true"/>      <filename condition="showDownload!='false'" label="Filename (without extension)" type="TextInput" defaultValue="model_answer_results" optional="true"/>      <fileOrientation condition="showDownload!='false'" label="File orientation" data="portrait,landscape" type="ComboBox" options="Portrait,Landscape" width="120" defaultValue="portrait" optional="true" tooltip="Orientation of downloaded file"/>    </downloadGroup>    <copypasteinfo label="Instructions" type="TextArea" height="50" defaultValue="Note: Click the 'Select Text' button to highlight the text on the right and then Ctrl + C to copy this text to the clipboard. You can then paste (Ctrl + V) into another application such as Open Office, Word or an email to save for future reference." optional="true" deprecated="This property is deprecated - use the 'Copy Button' optional property to add a button that copies responses to the clipboard" />    <copyBtn2Label label="Copy Button Label" type="TextInput" wysiwyg="true" language="true" mandatory="true" defaultValue="Copy"/>    <downloadTxt label="Download Button Label" type="TextInput" condition="showDownload!='false'" mandatory="true" language="true" defaultValue="Download" />    <pageTxt label="Page Number Text" type="TextInput" condition="unviewed=='true'" mandatory="true" language="true" defaultValue="Page" />    <notViewed label="Not Viewed Text" type="TextInput" condition="unviewed=='true'" mandatory="true" language="true" defaultValue="Page not viewed" />    <copyBtnLabel label="Select Button Label" type="TextInput" wysiwyg="true" language="true" deprecated="This property is no longer in use"/>    <newNodes>        <modelAnswerPage><![CDATA[<modelAnswerPage name="Model Answer Page" page=""/>]]></modelAnswerPage>    </newNodes>  </modelAnswerResults>  <modelAnswerPage menuItem="Page Results" icon="icTextArea" remove="true">    <name label="Label" type="TextInput" wysiwyg="false" tooltip="The label only appears in the editor's project structure panel and is not shown in the project itself. Unique and meaningful labels can be used to make pages easier to locate for editing at a later date."/>    <page label="Page" type="pageList" pageTypes="modelAnswer" tooltip="The data from this Model Answer page will be shown on this page"/>  </modelAnswerPage></wizard>