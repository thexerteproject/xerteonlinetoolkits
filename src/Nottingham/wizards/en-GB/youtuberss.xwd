<wizard menus="Media">  <pageWizard remove="true" duplicate="false">    <newNodes>      <youtuberss><![CDATA[<youtuberss name="Enter Page Title" text="" url="" results="20" panelWidth="Medium"                           	  resultsTxt="YouTube results for term"                           	  apiErrorMesg="The Youtube API returned the following error: '{0}'&lt;br&gt;If no API key is set, ask your admin to set up a key in api_keys.php (see api_keys.dist.php for a template)." />]]></youtuberss>    </newNodes>  </pageWizard>  <youtuberss menuItem="YouTube Feed" menu="Media" remove="true" thumb="thumbs/youtubefeed.jpg" example="https://xot.xerte.org.uk/play_116?hide=bottom#youtubefeed" wiki="https://xerte.org.uk/wiki/doku.php?id=media_youtube_feed_page" hint="Include YouTube videos in your project. Enter a search term to display videos matching the term from YouTube." icon="icYoutube">    <name type="TextInput" wysiwyg="true" label="Page Title"/>    <text type="textArea" label="Instructions" height="200" placeholder="Enter text for the page here"/>    <url type="textInput" label="Search Term"/>    <results type="NumericStepper" min="5" max="50" step="1" label="Number of Results"/>	<panelWidth label="Panel Width" options="Small,Medium,Large,Full" type="ComboBox"  data="Small,Medium,Large,Full" defaultValue="Medium" width="100" mandatory="true"/>		<resultsTxt label="Results Text" type="TextInput" wysiwyg="true" divider="true" language="true" />    <apiErrorMesg label="Google Youtube API error message" type="textInput" language="true"/>  </youtuberss></wizard>